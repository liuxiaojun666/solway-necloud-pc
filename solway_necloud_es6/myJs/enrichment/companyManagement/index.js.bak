ajaxData({
    companyList:{
        name: 'GETcompanyList',
        data: {},
        later: true
    }
}, {})('companyManagementCtrl', ['$scope', 'myAjaxData', 'actionRecord', '$timeout','toaster'], ($scope, myAjaxData, historicalRecord, $timeout,toaster) => {
    $scope.moduleName = '集团管理';//当前页面名称；

    //列表
    $scope.getCompanyList = () =>{
        $scope.companyList.getData({
            pageIndex: 0,
            pageSize: 10,
            keyWords:$scope.keyWords,
            // stClass:$scope.stClass
        }).then(res=>{
            console.log(res);
        });
    }

    $scope.getCompanyList();

    $scope.column = [
        {
            title: '集团名称',
            dataIndex: 'comName',
            align: 'center',
        },{
            title: '最早接入时间',
            dataIndex: 'comUpdateTime',
            align: 'center',
            render(text, record) {
                return text && new Date(text).Format('yyyy-MM-dd hh:mm:ss')
            }
        }, {
            title: '装机容量',
            dataIndex: 'comName',
            align: 'center',
        }, {
            title: '电站数量',
            dataIndex: 'comName',
            align: 'center',
        }, {
            title: '联系人',
            dataIndex: 'comCont',
            align: 'center',
            
        }, {
            title: '联系电话',
            dataIndex: 'comTel',
            align: 'center',
        }
    ]

  

    $timeout(() => {
        $('.ng-clock').removeClass('ng-clock');
    },2000);
});