ajaxData({
    companyListNew:{
        name: 'GETcompanyListNew',
        data: {},
        later: true
    }
}, {})('companyManagementCtrl', ['$scope', 'myAjaxData', 'actionRecord', '$timeout','toaster'], ($scope, myAjaxData, historicalRecord, $timeout,toaster) => {
    $scope.moduleName = '集团管理';//当前页面名称；

    //列表
    $scope.getCompanyList = () =>{
        $scope.loading = true;
        $scope.companyListNew.getData({
            pageIndex: 0,
            pageSize: 10,
            comName:$scope.keyWords,
        })
    }

    $scope.companyListNew.subscribe(res=>{
        $scope.loading = false;
    })

    $scope.getCompanyList();

    $scope.column = [
        {
            title: '集团名称',
            dataIndex: 'COM_NAME',
            align: 'center',
        },{
            title: '最早接入时间',
            dataIndex: 'expiryDate',
            align: 'center',
            render(text, record) {
                return text?new Date(text).Format('yyyy-MM-dd'):'-'
            }
        }, {
            title: '装机容量',
            dataIndex: 'BuildCapacity',
            align: 'center',
        }, {
            title: '电站数量',
            dataIndex: 'count00',
            align: 'center',
        }, {
            title: '联系人',
            dataIndex: 'COM_CONT',
            align: 'center',
            
        }, {
            title: '联系电话',
            dataIndex: 'COM_TEL',
            align: 'center',
        }
    ]

  

    $timeout(() => {
        $('.ng-clock').removeClass('ng-clock');
    },2000);
});