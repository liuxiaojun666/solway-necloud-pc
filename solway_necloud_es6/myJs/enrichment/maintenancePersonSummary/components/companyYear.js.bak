ajaxData({
    //公司----年

    //时间
    analysis_company_operation_time_year_list: {
        name: 'GETanalysis_company_operation_time_year_list',
        data: {},
        later: true,
        onlyLatest: true
    },
    //电站
    analysis_company_operation_station_year_list: {
        name: 'GETanalysis_company_operation_station_year_list',
        data: {},
        later: true,
        onlyLatest: true
    },
    //专业人员
    analysis_company_operation_person_year_list: {
        name: 'GETanalysis_company_operation_person_year_list',
        data: {},
        later: true,
        onlyLatest: true
    },
    //共享人员
    sharePeopleList:{
        name: 'GETsharePeopleList',
        data: {},
        later: true,
        onlyLatest: true
    }
}, {
    __serviceName__: 'maintenancePersonCompanyYear',
})('maintenancePersonCompanyYearCtrl', ['$scope', 'maintenancePersonCompanyYear', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { tabIndex = 0 } = historicalRecord.get().themeMaintenanceCompanyYear || {};
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        // await $scope.dateUpdated();
        $scope.changeTab(tabIndex);
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData();
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => {
        $scope.diffGetData();
    };

    // 点击电站名  跳转到电站页面
    $scope.trClick_ = (item, index, event) => {
        if (!event.target.className.includes('clickable')) return;
        $scope.reloadParentPage({
            stationData: {
                dataType: 0,
                id: item.stid,
                isGroup: "0",
                stClass: '01'
            }
        });
    };

    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData();
    };


    // tab 切换
    $scope.changeTab = tabIndex => {
        $scope.tabIndex = tabIndex;
        historicalRecord.set({
            themeMaintenanceCompanyYear: {
                ...historicalRecord.get().themeMaintenanceCompanyYear || {},
                tabIndex
            }
        });
        $scope.diffGetData();
    };

    //监听刷新
    $scope.$on('refresh',()=>{
        $scope.brandCondition = [];
        $scope.brandArr = [];
        $scope.keywords = '';
        $scope.diffGetData();
    });

    //监听退回
    $scope.$on('beback',()=>{
        $scope.brandCondition = [];
        $scope.brandArr = [];
        $scope.keywords = '';
        $scope.diffGetData();
    });

    //监听查询-专业
    $scope.$on('search',(e,val)=>{
        $scope.brandCondition = val.brandCondition;
        $scope.brandArr = val.brandArr;
        $scope.diffGetData()
    });

    //监听查询-共享
    $scope.$on('searchShare',(e,val)=>{
        $scope.keywords = val.keywords;
        $scope.diffGetData()
    });


    $scope.trClick = (item,index,e) =>{
        e.stopPropagation();
        if($scope.tabIndex == 2 ){
            $scope.$broadcast('paramsPeopleInfoProfessional',{'userId':item.userid});
            $scope.showPeopleInfoProfessional = true;
        }else if($scope.tabIndex == 3){
            $scope.$broadcast('paramsPeopleInfoShare',{'userId':item.id,'phone':item.phone});
            $scope.showPeopleInfoShare = true;
        }
    }

    //清除专业---弹框的查询时间
    $scope.$watch('showPeopleInfoProfessional',(n,o)=>{
        if(!n){
            $scope.$broadcast('clearDate');
        }
    });

    //清除共享---弹框的查询时间
    $scope.$watch('showPeopleInfoShare',(n,o)=>{
        if(!n){
            $scope.$broadcast('clearDateShare');
        }
    });
    
    // 判断 调用  对应 接口
    $scope.diffGetData = async()=>{
        if (!$scope.initComplete) return;
        $scope.companyYearLoading = true;
        await myAjaxData.timeout(0);
        
        const {dateTime, tabIndex ,dateType, dataType} = $scope;
        if(dateType !== 2) return;
        if(!dataType) return; //公司

        switch (tabIndex) {
            case 0: //时间
                $scope.analysis_company_operation_time_year_list.getData({
                    queryCode: 'analysis_company_operation_time_year_list',
                    queryType: 1,
                    date: dateTime.showDate
                });
                $scope.tabName = "时间";                
                break;
            case 1: //电站
                $scope.analysis_company_operation_station_year_list.getData({
                    queryCode: 'analysis_company_operation_station_year_list',
                    queryType: 1,
                    date: dateTime.showDate
                });
                $scope.tabName = "电站";                
                break;
            case 2: //专业人员
                $scope.analysis_company_operation_person_year_list.getData({
                    queryCode: 'analysis_company_operation_person_year_list',
                    queryType: 1,
                    date: dateTime.showDate,
                    pageIndex: 0,
                    pageSize: 20
                });
                $scope.tabName = "专业人员";                
                break;
            case 3: //共享人员
                $scope.sharePeopleList.getData({
                    pageIndex: 0,
                    pageSize: 20,
                    keywords:$scope.keywords
                });
                $scope.tabName = "共享人员";                
                break;
            default:
                break;
        }
        
    }

    // 时间列表
    $scope.analysis_company_operation_time_year_list.getDataCallback = (success, res) => {
        $scope.companyYearLoading = false;

        if (!success) return;
        $scope.listSource = res.body.data;

        $scope.column = [
            {
                title: '时间',
                width: '25%',
                dataIndex: 'dtime',
                align: 'left',
                // render(text, record, index) {
                //     return `<span class="alert-name" id="${record.id}">${text}</span>`
                // }
            },
            {
                title: '检修台次',
                dataIndex: 'repair_count',
                align: 'right',
            },
            {
                title: '检修台数',
                dataIndex: 'repair_number',
                align: 'right',
            },
            {
                title: '响应时间'+ '(' + res.body.units.res_hours_avg + ')',
                dataIndex: 'res_hours_avg',
                sort: 'true',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`
                }
            },
            {
                title: '检修时间'+ '(' + res.body.units.repair_hours_avg + ')',
                dataIndex: 'repair_hours_avg',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`
                }
            },
            {
                title: '检修成功率'+ '(' + res.body.units.repair_finish_r + ')',
                dataIndex: 'repair_finish_r',
                sort: 'true',
                align: 'right'
            }
        ];
        
        // $scope.selectedBrands = [],$scope.selectedFaults = [];
        // $scope.$watch("selectedBrands", function(n, o){
        //     // console.log(n)
        // })
        // $scope.$watch("selectedFaults", function(n, o){
        //     // console.log(n)
        // })

        // $(".maintenanceStationMonth").on('click','.alert-name',function(){
        //     let a = $(this).attr('id')
        // })
        // 
        // 
    };

    // 电站列表
    $scope.analysis_company_operation_station_year_list.getDataCallback = (success, res) => {
        $scope.companyYearLoading = false;

        if (!success) return;
        $scope.listSource = res.body.data;
        
        $scope.column = [
            {
                title: '电站',
                width: '25%',
                dataIndex: 'st_name',
                align: 'left',
                render(text) {
                    return `<span class="clickable">${text}</span>`
                }
            },
            {
                title: '检修台次',
                dataIndex: 'repair_count',
                align: 'right',
            },
            {
                title: '检修台数',
                dataIndex: 'repair_number',
                align: 'right',
            },
            {
                title: '响应时间'+ '(' + res.body.units.res_hours_avg + ')',
                dataIndex: 'res_hours_avg',
                sort: 'true',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`
                }
            },
            {
                title: '检修时间'+ '(' + res.body.units.repair_hours_avg + ')',
                dataIndex: 'repair_hours_avg',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`
                }
            },
            {
                title: '检修成功率'+ '(' + res.body.units.repair_finish_r + ')',
                dataIndex: 'repair_finish_r',
                sort: 'true',
                align: 'right'
            }
        ];
        
        // $scope.selectedBrands = [],$scope.selectedFaults = [];
        // $scope.$watch("selectedBrands", function(n, o){
        //     // console.log(n)
        // })
        // $scope.$watch("selectedFaults", function(n, o){
        //     // console.log(n)
        // })

        // $(".maintenanceStationMonth").on('click','.alert-name',function(){
        //     let a = $(this).attr('id')
        // })
        // 
        // 
    };

    // 专业人员列表
    $scope.analysis_company_operation_person_year_list.getDataCallback = (success, res) => {
        $scope.companyYearLoading = false;

        if (!success) return;
        $scope.listSource = res.body.data.data;

        $scope.column = [
            {
                title: '姓名',
                width: '25%',
                dataIndex: 'user_name',
                align: 'center',
                render(text, record, index) {
                    return `<span style="color:#1ddabe" id="${record.id}">${text}</span>`
                }
            },
            {
                title: '检修台次',
                dataIndex: 'repair_count',
                align: 'right',
            },
            {
                title: '检修台数',
                dataIndex: 'repair_number',
                align: 'right',
            },
            {
                title: '响应时间'+ '(' + res.body.units.res_hours_avg + ')',
                dataIndex: 'res_hours_avg',
                sort: 'true',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`
                }
            },
            {
                title: '检修时间'+ '(' + res.body.units.repair_hours_avg + ')',
                dataIndex: 'repair_hours_avg',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`
                }
            },
            {
                title: '检修成功率'+ '(' + res.body.units.repair_finish_r + ')',
                dataIndex: 'repair_finish_r',
                sort: 'true',
                align: 'right'
            }
        ];
        
        // $scope.selectedBrands = [],$scope.selectedFaults = [];
        // $scope.$watch("selectedBrands", function(n, o){
        //     // console.log(n)
        // })
        // $scope.$watch("selectedFaults", function(n, o){
        //     // console.log(n)
        // })

        // $(".maintenanceStationMonth").on('click','.alert-name',function(){
        //     let a = $(this).attr('id')
        // })
        // 
        // 
    };

    //共享人员列表
    $scope.sharePeopleList.getDataCallback = (success, res) => {

        $scope.companyYearLoading = false;
        if (!success) return;
        $scope.listSource = res.body.data;
        $scope.column = [
            {
                title: '姓名',
                width: '25%',
                dataIndex: 'realName',
                align: 'center',
                render(text, record, index) {
                    return `<span style="color:#1ddabe" id="${record.id}">${text}</span>`
                }
            },
            {
                title: '手机号',
                dataIndex: 'phone',
                align: 'center',
            },
            {
                title: '抢单次数',
                dataIndex: 'h_num',
                align: 'right',
            },
            {
                title: '响应时间(h)',
                dataIndex: 'res_hours',
                sort: 'true',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`

                }
            },
            {
                title: '处理时间(h)',
                dataIndex: 'repair_hours',
                align: 'right',
                render(text, record, index) {
                    return `<span>${text===null?0:text}</span>`

                }
            },
            // {
            //     title: '成功率(%)',
            //     dataIndex: '',
            //     sort: 'true',
            //     align: 'right'
            // }
        ];
    }




    // 销毁 事件
    $scope.$on('$destroy', () => {
    });
});