    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { tabIndex = 0 } = historicalRecord.get().themeMaintenanceStationMonth || {};
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        await $scope.dateUpdated();
        $scope.changeTab(tabIndex);
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData(true);
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => {
        $scope.diffGetData();
    };

    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData(true);
    };


    // tab 切换
    $scope.changeTab = tabIndex => {
        $scope.tabIndex = tabIndex;
        historicalRecord.set({
            themeMaintenanceStationMonth: {
                ...historicalRecord.get().themeMaintenanceStationMonth || {},
                tabIndex
            }
        });
        $scope.diffGetData();
    };

    $scope.trClick = (item,index) =>{
        if($scope.tabIndex == 2 || $scope.tabIndex == 3){
            $scope.showPeopleInfo = true;
        }
    }
    // 判断 调用  对应 接口
    $scope.diffGetData = ()=>{
        if (!$scope.initComplete) return;

        const {dateTime, tabIndex } = $scope;
        switch (tabIndex) {
            case 0: //时间
                $scope.analysis_station_operation_time_month_list.getData({
                    queryCode: 'analysis_station_operation_time_month_list',
                    queryType: 3,
                    date: dateTime.showDate
                });
                break;
            case 1: //设备
                $scope.analysis_station_operation_dev_month_list.getData({
                    queryCode: 'analysis_station_operation_dev_month_list',
                    queryType: 3,
                    date: dateTime.showDate
                });
                break;
            case 2: //专业人员
                $scope.analysis_station_operation_person_month_list.getData({
                    queryCode: 'analysis_station_operation_person_month_list',
                    queryType: 3,
                    date: dateTime.showDate,
                    pageIndex: 0,
                    pageSize: 20
                });
                break;
            case 3: //共享人员
                test();
                break;
            default:
                break;
        }
        
    }

