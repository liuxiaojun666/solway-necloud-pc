app.directive('chartSelectSwitch', ['myAjaxData', '$ocLazyLoad', (myAjaxData, $ocLazyLoad) => ({
    restrict: 'E',
    replace: !0,
    template: `
    <div style="position: relative;z-index: 10;">
        <select 
            ng-show="select1"
            ng-change="selectChange('select1');" 
            ng-model="select1" 
            style="background: #16385d;border-color: #5179aa; width: 150px;height: 2em;position: absolute;left: 30px;top: 25px;">
            <option 
                ng-repeat="(key, value) in select1Options" 
                ng-selected="key == select1" 
                value="{{key}}"
                >{{value}}</option>
        </select>
        <select 
            ng-show="select2"
            ng-change="selectChange('select2');" 
            ng-model="select2" 
            style="background: #16385d;border-color: #5179aa; width: 150px;height: 2em;position: absolute;right: 30px;top: 25px;">
            <option 
                ng-repeat="(key, value) in select2Options" 
                ng-selected="key == select2" 
                value="{{key}}"
                >{{value}}</option>
        </select>
    </div>`,
    scope: {
        select1: '=',
        select2: '=',
        selectChange: '=',
        select1Options: '=',
        select2Options: '='
    },
    link($scope, $element, $attrs) {

    },
    controller($scope) {
        
    }
})])