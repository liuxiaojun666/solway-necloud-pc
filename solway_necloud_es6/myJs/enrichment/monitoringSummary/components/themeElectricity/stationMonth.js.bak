ajaxData({
    // 电站 月 时间 列表
    query_station_month_elec_detail_timelist_his: {
        name: 'GETquery_station_month_elec_detail_timelist_his',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 电站 月 时间 曲线
    query_station_month_elec_detail_timeline_his: {
        name: 'GETquery_station_month_elec_detail_timeline_his',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 电站 月 设备 列表
    query_station_month_elec_devlist_his: {
        name: 'GETquery_station_month_elec_devlist_his',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 电站 月 设备 曲线
    query_station_month_elec_devline_his: {
        name: 'GETquery_station_month_elec_devline_his',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 电站 月 结算电量 
    opSettlementPowerAnalysisByNumMonth: {
        name: 'GETopSettlementPowerAnalysisByNumMonth',
        data: {},
        later: true,
        onlyLatest: true
    },
}, {
        __serviceName__: 'themeElectricityStationMonthService'
    })('themeElectricityStationMonthCtrl', ['$scope', 'themeElectricityStationMonthService', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
        // 初始化 scope
        $scope.initChildScope($scope, myAjaxData);

        // 主页面初始化完成 回调
        $scope.mainPageInitComplete = async () => {
            let { showShape, select1, select2, tabIndex = 0 } = historicalRecord.get().themeElectricityStationMonth || {};
            $scope.changeShowShape(showShape || 'list');
            const { dateType, dateTime } = historicalRecord.get();
            $scope.dateType = dateType;
            $scope.dateTime = dateTime;
            await $scope.dateUpdated();
            $scope.select1 = select1 || 2;
            $scope.select2 = select2 || 2;
            if (select1) await $scope.selectChange('select1');
            if (select2) await $scope.selectChange('select2');
            $scope.changeTab(tabIndex);
            $('.ng-clock').removeClass('ng-clock');
            $scope.initComplete = true;
            await myAjaxData.timeout(0);
            await $scope.diffGetData();
            $scope.$apply();
        };

        // 切换电站 回调
        $scope.switchPowerCallback = () => {
            if ($scope.currentClass !== 'StationMonth') return;
            if ($scope.dataType > 0) return;
            setDataNull();
            diffSelectOptions();
            $scope.diffGetData();
        };

        // 日期改变
        $scope.dateUpdated = async () => {
            if ($scope.currentClass !== 'StationMonth') return;
            await myAjaxData.timeout(0);
            setDataNull();
            diffSelectOptions();
            $scope.diffGetData();
        };


        // tab 切换
        $scope.changeTab = tabIndex => {
            $scope.tabIndex = tabIndex;
            historicalRecord.set({
                themeElectricityStationMonth: {
                    ...historicalRecord.get().themeElectricityStationMonth || {},
                    tabIndex
                }
            });
            setDataNull();
            diffSelectOptions();
            $scope.diffGetData();
        };

        // 切换显示形式  图表 or 列表
        $scope.changeShowShape = showShape => {
            $scope.showShape = showShape;
            historicalRecord.set({
                themeElectricityStationMonth: {
                    ...historicalRecord.get().themeElectricityStationMonth || {},
                    showShape
                }
            });
            setDataNull();
            diffSelectOptions();
            $scope.diffGetData();
        };

        // 图标轴线切换
        $scope.selectChange = async (arg) => {
            await myAjaxData.timeout(0);
            historicalRecord.set({
                themeElectricityStationMonth: {
                    ...historicalRecord.get().themeElectricityStationMonth || {},
                    [arg]: $scope[arg]
                }
            });
            diffChartLineData();
            $scope.$apply();
        };

        $scope.diffGetData = async () => {
            if (!$scope.initComplete) return;
            await myAjaxData.timeout(0);
            const { currentClass, showShape, dateTime, tabIndex, dateType } = $scope;
            if (showShape === 'list' && tabIndex === 0) {
                $scope.query_station_month_elec_detail_timelist_his.getData({
                    queryCode: 'query_station_month_elec_detail_timelist_his',
                    queryType: 3,
                    date: dateTime.showDate
                });
            }
            else if (showShape === 'chart' && tabIndex === 0) {
                $scope.query_station_month_elec_detail_timeline_his.getData({
                    queryCode: 'query_station_month_elec_detail_timeline_his',
                    queryType: 3,
                    date: dateTime.showDate
                });
            }
            else if (showShape === 'list' && tabIndex === 1) {
                $scope.query_station_month_elec_devlist_his.getData({
                    queryCode: 'query_station_month_elec_devlist_his',
                    queryType: 3,
                    date: dateTime.showDate
                });
            }
            else if (showShape === 'chart' && tabIndex === 1) {
                $scope.query_station_month_elec_devline_his.getData({
                    queryCode: 'query_station_month_elec_devline_his',
                    queryType: 3,
                    date: dateTime.showDate
                });
            }
            else if (tabIndex === 2) {
                $scope.opSettlementPowerAnalysisByNumMonth.getData({
                    queryDate: dateTime.showDate
                });
            }
        }

        // 电站 月 时间 列表
        $scope.query_station_month_elec_detail_timelist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.column = [
                {
                    title: '时间',
                    dataIndex: 'dtime',
                    width: '20%',
                    sort: true,
                    align: 'center'
                },
                {
                    title: '实发电量' + (res.body.units.real_kwh_chk ? '(' + res.body.units.real_kwh_chk + ')' : ''),
                    dataIndex: 'real_kwh_chk',
                    width: '20%',
                    sort: true,
                    align: 'right'
                },
                {
                    title: '应发电量' + (res.body.units.shd_kwh_chk ? '(' + res.body.units.shd_kwh_chk + ')' : ''),
                    dataIndex: 'shd_kwh_chk',
                    width: '20%',
                    sort: true,
                    align: 'right'
                },
                {
                    title: '上网电量' + (res.body.units.send_kwh_chk ? '(' + res.body.units.send_kwh_chk + ')' : ''),
                    dataIndex: 'send_kwh_chk',
                    width: '20%',
                    sort: true,
                    align: 'right'
                },
                {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    width: '10%',
                    sort: true,
                    align: 'right',
                },
                {
                    title: 'PR' + (res.body.units.pr ? '(' + res.body.units.pr + ')' : ''),
                    dataIndex: 'pr',
                    width: '10%',
                    sort: true,
                    align: 'right',
                },
                {
                    title: '损失百分比' + (res.body.units.we_rate ? '(' + res.body.units.we_rate + ')' : ''),
                    dataIndex: 'we_rate',
                    width: '20%',
                    sort: true,
                    align: 'right',
                },
            ];
        };
        // 电站 月 时间 图表
        $scope.query_station_month_elec_detail_timeline_his.getDataCallback = async (success, res) => {
            if (!success) return;
            $scope.select1 = $scope.selectOptions[$scope.select1 || '__'] ? $scope.select1 : 'real_kwh_chk';
            $scope.select2 = $scope.selectOptions[$scope.select2 || '__'] ? $scope.select2 : 'shd_kwh_chk';
            await $scope.selectChange('select1');
            await $scope.selectChange('select2');
            diffSelectOptions(res.body.units);
            diffChartLineData();
            $scope.$apply();
        };
        // 电站 月 设备 列表
        $scope.query_station_month_elec_devlist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.column = [
                {
                    title: '设备名称',
                    dataIndex: 'eq_name',
                    width: '25%',
                    sort: true,
                },
                {
                    title: '装机容量' + (res.body.units.inst_kw ? '(' + res.body.units.inst_kw + ')' : ''),
                    dataIndex: 'inst_kw',
                    width: '15%',
                    sort: true,
                    align: 'right'
                },
                {
                    title: '发电量' + (res.body.units.real_kwh_chk ? '(' + res.body.units.real_kwh_chk + ')' : ''),
                    dataIndex: 'real_kwh_chk',
                    width: '15%',
                    sort: true,
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="同比${item.real_kwh_chk_tb_rate || '-'}%, 环比${item.real_kwh_chk_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                },
                {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    width: '15%',
                    sort: true,
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="同比${item.real_hours_tb_rate || '-'}%, 环比${item.real_hours_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                },
                {
                    title: 'PR' + (res.body.units.pr ? '(' + res.body.units.pr + ')' : ''),
                    dataIndex: 'pr',
                    width: '15%',
                    sort: true,
                    align: 'right',
                },
                {
                    title: '故障损失电量' + (res.body.units.fail_we ? '(' + res.body.units.fail_we + ')' : ''),
                    dataIndex: 'fail_we',
                    width: '15%',
                    sort: true,
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="同比${item.fail_we_tb_rate || '-'}%, 环比${item.fail_we_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                },
                {
                    title: '阴影损失电量' + (res.body.units.shade_we ? '(' + res.body.units.shade_we + ')' : ''),
                    dataIndex: 'shade_we',
                    width: '15%',
                    sort: true,
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="同比${item.shade_we_tb_rate || '-'}%, 环比${item.shade_we_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                },
                {
                    title: '灰尘损失电量' + (res.body.units.dust_we ? '(' + res.body.units.dust_we + ')' : ''),
                    dataIndex: 'dust_we',
                    width: '15%',
                    sort: true,
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="同比${item.dust_we_tb_rate || '-'}%, 环比${item.dust_we_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                },
            ]
        };
        // 电站 月 设备 曲线
        $scope.query_station_month_elec_devline_his.getDataCallback = async (success, res) => {
            if (!success) return;
            $scope.select1 = $scope.selectOptions[$scope.select1 || '__'] ? $scope.select1 : 'real_kwh_chk';
            $scope.select2 = $scope.selectOptions[$scope.select2 || '__'] ? $scope.select2 : 'inst_kw';
            await $scope.selectChange('select1');
            await $scope.selectChange('select2');
            diffSelectOptions(res.body.units);
            diffChartLineData();
            $scope.$apply();
        };

        // 电站 月 结算电量
        $scope.opSettlementPowerAnalysisByNumMonth.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.opSettlementPowerAnalysisByNumMonth.res.body.imgsArr = (res.body.imgs || '').split(',');
        };


        // 计算 图表 显示对应 轴
        function diffChartLineData() {
            if (!$scope.initComplete) return;
            const { select1, select2, tabIndex } = $scope;
            if (tabIndex === 0) {
                $scope.xdata = $scope.query_station_month_elec_detail_timeline_his.res.body.data.dtime
                $scope.ydatas = [[$scope.query_station_month_elec_detail_timeline_his.res.body.data[select1]], [$scope.query_station_month_elec_detail_timeline_his.res.body.data[select2]]]
            } else if (tabIndex === 1) {
                $scope.xdata = $scope.query_station_month_elec_devline_his.res.body.data.eq_name
                $scope.ydatas = [[$scope.query_station_month_elec_devline_his.res.body.data[select1]], [$scope.query_station_month_elec_devline_his.res.body.data[select2]]]
            }
        }

        // 轴线切换  可选项  判断
        function diffSelectOptions(units = {}) {

            if ($scope.tabIndex === 0) {
                $scope.selectOptions = {
                    real_kwh_chk: '实发电量' + (units.real_kwh_chk ? '(' + units.real_kwh_chk + ')' : ''),
                    send_kwh_chk: '上网电量' + (units.send_kwh_chk ? '(' + units.send_kwh_chk + ')' : ''),
                    shd_kwh_chk: '应发电量' + (units.shd_kwh_chk ? '(' + units.shd_kwh_chk + ')' : ''),
                    we_rate: '其他损失电量' + (units.we_rate ? '(' + units.we_rate + ')' : ''),
                };
            } else if ($scope.tabIndex === 1) {
                $scope.selectOptions = {
                    decay_we: '衰减损失电量' + (units.decay_we ? '(' + units.decay_we + ')' : ''),
                    dust_we: '灰尘损失电量' + (units.dust_we ? '(' + units.dust_we + ')' : ''),
                    fail_we: '故障损失电量' + (units.fail_we ? '(' + units.fail_we + ')' : ''),
                    inst_kw: '装机容量' + (units.inst_kw ? '(' + units.inst_kw + ')' : ''),
                    other_we: '其他损失电量' + (units.other_we ? '(' + units.other_we + ')' : ''),
                    real_hours: '发电小时数' + (units.real_hours ? '(' + units.real_hours + ')' : ''),
                    real_kwh_chk: '实发电量' + (units.real_kwh_chk ? '(' + units.real_kwh_chk + ')' : ''),
                    repair_we: '检修损失电量' + (units.repair_we ? '(' + units.repair_we + ')' : ''),
                    restrict_we: '限电损失电量' + (units.restrict_we ? '(' + units.restrict_we + ')' : ''),
                    shade_we: '阴影损失电量' + (units.shade_we ? '(' + units.shade_we + ')' : ''),
                };
            }

        }

        // 数据置空
        function setDataNull() {
            $scope.column = [];
            $scope.xdata = void 0;
            $scope.ydatas = void 0;
            $scope.query_station_month_elec_devlist_his.res = null;
            $scope.query_station_month_elec_devline_his.res = null;
            $scope.query_station_month_elec_detail_timelist_his.res = null;
            $scope.query_station_month_elec_detail_timeline_his.res = null;
            $scope.opSettlementPowerAnalysisByNumMonth.res = null;

        }
    });