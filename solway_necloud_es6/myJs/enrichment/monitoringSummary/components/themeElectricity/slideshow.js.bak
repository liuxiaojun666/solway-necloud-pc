app.directive('themeElectricitySlideshow', ['$timeout', ($timeout) => ({
    restrict: 'E',
    replace: !0,
    templateUrl: 'tpl/enrichment/monitoringSummary/components/themeElectricity/slideshow.html',
    scope: {
        imgs: '='
    },
    link($scope, $element, $attrs) {
        $timeout(() => {
            $element.css('display', 'block');
        }, 0)
    },
})])