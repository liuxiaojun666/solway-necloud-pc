    },
    // 集团月资源详情电站列表
    query_company_month_res_stationlist_his: {
        name: 'GETquery_company_month_res_stationlist_his',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 集团月资源详情电站曲线
    query_company_month_res_stationline_his: {
