        data: {},
        later: true,
        onlyLatest: true
    },
}, {
        __serviceName__: 'themeResourceDetailService'
    })('themeResourceDetailCtrl', ['$scope', 'themeResourceDetailService', 'actionRecord', '$interval'], ($scope, myAjaxData, historicalRecord, $interval) => {
        // 初始化 scope
        $scope.initChildScope($scope, myAjaxData);

        // 主页面初始化完成 回调
        $scope.mainPageInitComplete = async () => {
            let { showShape, select1, select2, tabIndex = 0 } = historicalRecord.get().themeResource || {};
            const { dateType, dateTime } = historicalRecord.get();
            $scope.dateType = dateType;
            $scope.dateTime = dateTime;
            await $scope.dateUpdated();
            if (($scope.dateType === 0) || ($scope.dataType > 0 && showShape === 'chart2')) showShape = 'list';
            $scope.changeShowShape(showShape || 'list');
            $scope.select1 = select1;
            $scope.select2 = select2;
            if (select1) await $scope.selectChange('select1');
            if (select2) await $scope.selectChange('select2');
            $scope.changeTab(tabIndex);
            $('.ng-clock').removeClass('ng-clock');
            $scope.initComplete = true;
            await myAjaxData.timeout(0);
            diffGetData();
            $scope.$apply();
        };

        // 切换电站 回调
        $scope.switchPowerCallback = () => {
            if (($scope.dateType === 0) || ($scope.dataType > 0 && $scope.showShape === 'chart2')) $scope.changeShowShape('list');
            $scope.xdata = void 0;
            $scope.ydatas = void 0;
            diffSelectOptions();
            diffGetData();
        };

        // 日期改变
        $scope.dateUpdated = async () => {
            await myAjaxData.timeout(0);
            if ($scope.dateType === 0 || ($scope.currentClass === 'StationYear' && $scope.showShape === 'chart2')) $scope.changeShowShape('list');
            $scope.xdata = void 0;
            $scope.ydatas = void 0;
            diffSelectOptions();
            diffGetData();
        };

        // tab 切换
        $scope.changeTab = tabIndex => {
            if (tabIndex === $scope.tabIndex) return;
            $scope.tabIndex = tabIndex;
            historicalRecord.set({
                themeResource: {
                    ...historicalRecord.get().themeResource || {},
                    tabIndex
                }
            });
            diffGetData();
        };

        // 切换显示形式  图表 or 列表
        $scope.changeShowShape = showShape => {
            if (showShape === $scope.showShape) return;
            $scope.showShape = showShape;
            historicalRecord.set({
                themeResource: {
                    ...historicalRecord.get().themeResource || {},
                    showShape
                }
            });
            if ($scope.currentClass === 'CompanyDayAgo') return;
            diffGetData();
        };

        // 图标轴线切换
        $scope.selectChange = async (arg) => {
            await myAjaxData.timeout(0);
            historicalRecord.set({
                themeResource: {
                    ...historicalRecord.get().themeResource || {},
                    [arg]: $scope[arg]
                }
            });
            diffChartLineData();
            $scope.$apply();
        };

        // 判断 调用  对应 接口
        async function diffGetData() {
            if (!$scope.initComplete) return;
            await myAjaxData.timeout(0);
            const { currentClass, showShape, dateTime, tabIndex, dateType } = $scope;
            if (!currentClass.includes('DayToday')) $scope.column = [];
            switch (currentClass) {
                case 'StationDayToday':
                    $scope.stationDay.getData({
                        queryType: 3,
                        dateType: 3,
                        date: dateTime.showDate
                    });
                    break;

                case 'StationDayAgo':
                    $scope.query_station_day_res_timeone_his.getData({
                        queryType: 3,
                        queryCode: 'query_station_day_res_timeone_his',
                        date: dateTime.showDate
                    })
                    break;

                case 'StationMonth':
                    if (showShape === 'list') {
                        $scope.query_station_month_res_timelist_his.getData({
                            queryCode: 'query_station_month_res_timelist_his',
                            queryType: 3,
                            date: dateTime.showDate
                        });
                    } else {
                        $scope.query_station_month_res_timeline_his.getData({
                            queryCode: 'query_station_month_res_timeline_his',
                            queryType: 3,
                            date: dateTime.showDate
                        });
                    }
                    break;

                case 'StationYear':
                    if (showShape === 'list') {
                        $scope.query_station_year_res_timelist_his.getData({
                            queryCode: 'query_station_year_res_timelist_his',
                            queryType: 3,
                            date: dateTime.showDate
                        });
                    } else {
                        $scope.query_station_year_res_timeline_his.getData({
                            queryCode: 'query_station_year_res_timeline_his',
                            queryType: 3,
                            date: dateTime.showDate
                        });
                    }
                    break;

                case 'CompanyDayToday':
                    $scope.companyDayToday.getData({
                        queryType: 1,
                        dateType: 3,
                        date: dateTime.showDate
                    });
                    break;

                case 'CompanyDayAgo':
                    $scope.query_company_day_res_timelist_his.getData({
                        queryCode: 'query_company_day_res_timelist_his',
                        queryType: 1,
                        date: dateTime.showDate
                    });
                    break;

                case 'CompanyMonth':
                    if (tabIndex === 0 && showShape === 'list') {
                        $scope.query_company_month_res_timelist_his.getData({
                            queryCode: 'query_company_month_res_timelist_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    } else if (tabIndex === 0 && showShape === 'chart') {
                        $scope.query_company_month_res_timeline_his.getData({
                            queryCode: 'query_company_month_res_timeline_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    } else if (tabIndex === 1 && showShape === 'list') {
                        $scope.query_company_month_res_stationlist_his.getData({
                            queryCode: 'query_company_month_res_stationlist_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    } else if (tabIndex === 1 && showShape === 'chart') {
                        $scope.query_company_month_res_stationline_his.getData({
                            queryCode: 'query_company_month_res_stationline_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    }
                    break;

                case 'CompanyYear':
                    if (tabIndex === 0 && showShape === 'list') {
                        $scope.query_company_year_res_timelist_his.getData({
                            queryCode: 'query_company_year_res_timelist_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    } else if (tabIndex === 0 && showShape === 'chart') {
                        $scope.query_company_year_res_timeline_his.getData({
                            queryCode: 'query_company_year_res_timeline_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    } else if (tabIndex === 1 && showShape === 'list') {
                        $scope.query_company_year_res_stationlist_his.getData({
                            queryCode: 'query_company_year_res_stationlist_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    } else if (tabIndex === 1 && showShape === 'chart') {
                        $scope.query_company_year_res_stationline_his.getData({
                            queryCode: 'query_company_year_res_stationline_his',
                            queryType: 1,
                            date: dateTime.showDate
                        });
                    }
                    break;

                default:
                    break;
            }
        };

        // 电站月 列表 数据处理
        $scope.query_station_month_res_timelist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.column = [
                {
                    title: '时间',
                    dataIndex: 'dtime',
                    sort: 'true',
                    align: 'center',
                    style: 'position:relative;',
                    render(text, item, index) {
                        return text + `
                            <svg style="width: 30px;height: 30px;cursor:pointer;position: absolute;right: 1em;top:10px;" class="symbol-icon" aria-hidden="true">
                                <use xlink:href="#icon-${['', 'qingtian', 'yintian', 'yu-', 'xue-', 'wumai', 'other'][item.weather_code]}"></use>
                            </svg>`;
                    }
                },
                {
                    title: '辐射总量' + (res.body.units.rad_chk ? '(' + res.body.units.rad_chk + ')' : ''),
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '日照小时数' + (res.body.units.sun_hours ? '(' + res.body.units.sun_hours + ')' : ''),
                    dataIndex: 'sun_hours',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '最大光照强度' + (res.body.units.max_sun ? '(' + res.body.units.max_sun + ')' : ''),
                    dataIndex: 'max_sun',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '平均温度' + (res.body.units.ta_avg ? '(' + res.body.units.ta_avg + ')' : ''),
                    dataIndex: 'ta_avg',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '降雨量' + (res.body.units.pcpn ? '(' + res.body.units.pcpn + ')' : ''),
                    dataIndex: 'pcpn',
                    sort: 'true',
                    align: 'center',
                },
            ];
        };
        // 电站年 列表 数据处理
        $scope.query_station_year_res_timelist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.column = [
                {
                    title: '时间',
                    dataIndex: 'dtime',
                    sort: 'true',
                    align: 'center',
                },
                {
                    title: '辐射总量(' + res.body.units.rad_chk + ')',
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '发电小时数(' + res.body.units.real_hours + ')',
                    dataIndex: 'real_hours',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '日照小时数(' + res.body.units.sun_hours + ')',
                    dataIndex: 'sun_hours',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '降雨量(' + res.body.units.pcpn + ')',
                    dataIndex: 'pcpn',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '平均温度',
                    dataIndex: 'ta_avg',
                    sort: 'true',
                    align: 'right',
                },
            ];
        };
        // 电站月 图表 数据处理
        $scope.query_station_month_res_timeline_his.getDataCallback = async (success, res) => {
            if (!success) return;
            if (!res.body.data.dtime[0]) return;
            $scope.select1 = $scope.selectOptions[$scope.select1 || '__'] ? $scope.select1 : 'rad_chk';
            $scope.select2 = $scope.selectOptions[$scope.select2 || '__'] ? $scope.select2 : 'sun_hours';
            $scope.query_station_month_res_timeline_his.res.body.data.nullBlock = [];
            $scope.query_station_month_res_timeline_his.res.body.data.nullBlock2 = [];
            await $scope.selectChange('select1');
            await $scope.selectChange('select2');
            const day = new Date($scope.dateTime.showDate.split('-')).getDay() - 1;
            $scope.query_station_month_res_timeline_his.res.body.data.nullBlock = new Array(day < 0 ? 6 : day).fill('');
            if ($scope.dateTime.showDate === (new Date).Format('yyyy-MM')) {
                const date = new Date();
                const dateNum = date.getDate();
                $scope.query_station_month_res_timeline_his.res.body.data.nullBlock2 = new Array(new Date(date.getFullYear(), date.getMonth() + 1, 0).getDate() - dateNum + 1).fill('').map((v, i) => (i + dateNum) < 10 ? '0' + (i + dateNum) : i + dateNum);
            }
            diffSelectOptions(res.body.units);
            diffChartLineData();
            $scope.$apply();
        };
        // 电站年 图表 数据回调 处理
        $scope.query_station_year_res_timeline_his.getDataCallback = async (success, res) => {
            if (!success) return;
            if (!res.body.data.dtime[0]) return;
            $scope.select1 = $scope.selectOptions[$scope.select1 || '__'] ? $scope.select1 : 'rad_chk';
            $scope.select2 = $scope.selectOptions[$scope.select2 || '__'] ? $scope.select2 : 'sun_hours';
            await $scope.selectChange('select1');
            await $scope.selectChange('select2');
            diffSelectOptions(res.body.units);
            diffChartLineData();
            $scope.$apply();
        };
        // 公司 今日 列表 数据处理
        $scope.companyDayToday.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.column = [
                {
                    title: '电站名称',
                    dataIndex: 'st_name',
                    sort: 'true',
                    width: '25%',
                    align: 'left'
                },
                {
                    title: '装机容量(kW)',
                    dataIndex: 'inst_kw',
                    sort: 'true',
                    width: '10%',
                    align: 'right'
                },
                {
                    title: '辐射总量(kWh/m²)',
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    width: '15%',
                    align: 'right'
                },
                {
                    title: '光照强度(W/m²)',
                    dataIndex: 'sun',
                    sort: 'true',
                    width: '10%',
                    align: 'right'
                },
                {
                    title: '天气',
                    dataIndex: 'weater',
                    sort: 'true',
                    width: '10%',
                    align: 'center'
                },
                {
                    title: '温度(℃)',
                    dataIndex: 'tmp',
                    sort: 'true',
                    width: '10%',
                    align: 'center',
                },
                {
                    title: '预测今日',
                    dataIndex: 'pre_weater',
                    sort: 'true',
                    width: '10%',
                    align: 'center',
                    render(text, item) {
                        return text ? '<span>' + text + ' ' + item.pre_tmp  + '℃'+ '</span>' : '<span></span>';
                    }
                },
                {
                    title: '预测明日',
                    dataIndex: 'pre_weater2',
                    sort: 'true',
                    width: '10%',
                    align: 'center',
                    render(text, item) {
                        return text ? '<span>' + text + ' ' + item.pre_tmp2 + '℃' + '</span>' : '<span></span>';
                    }
                },
                {
                    title: '预测后日',
                    dataIndex: 'pre_weater3',
                    sort: 'true',
                    width: '10%',
                    align: 'center',
                    render(text, item) {
                        return text ? '<span>' + text + ' ' + item.pre_tmp3 + '℃' + '</span>' : '<span></span>';
                    }
                },
            ];
        };
        // 公司 历史日 列表 数据回调 处理
        $scope.query_company_day_res_timelist_his.getDataCallback = async (success, res) => {
            if (!success) return;
            $scope.select1 = $scope.selectOptions[$scope.select1 || '__'] ? $scope.select1 : 'inst_kw';
            await $scope.selectChange('select1');
            $scope.xdata = res.body.data.map(v => v.st_name);
            $scope.ydatas = [[res.body.data.map(v => v[$scope.select1])], [res.body.data.map(v => v.max_sun)]];
            $scope.column = [
                {
                    title: '电站名称',
                    dataIndex: 'st_name',
                    sort: 'true',
                    width: '30%',
                    align: 'left',
                },
                {
                    title: '装机容量' + (res.body.units.inst_kw ? '(' + res.body.units.inst_kw + ')' : ''),
                    dataIndex: 'inst_kw',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '辐射总量' + (res.body.units.rad_chk ? '(' + res.body.units.rad_chk + ')' : ''),
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '最大光照强度' + (res.body.units.max_sun ? '(' + res.body.units.max_sun + ')' : ''),
                    dataIndex: 'max_sun',
                    sort: 'true',
                    align: 'right',
                    width: '15%',
                },
                {
                    title: '天气',
                    dataIndex: 'weather',
                    sort: 'true',
                    align: 'center',
                },
                {
                    title: '温度' + (res.body.units.ta_avg ? '(' + res.body.units.ta_avg + ')' : ''),
                    dataIndex: 'ta_avg',
                    sort: 'true',
                    align: 'right',
                },
                {
                    title: '降雨量' + (res.body.units.pcpn ? '(' + res.body.units.pcpn + ')' : ''),
                    dataIndex: 'pcpn',
                    sort: 'true',
                    align: 'center',
                },
            ];
            diffSelectOptions(res.body.units);
            $scope.$apply();
        };
        // 公司 月 时间 列表
        $scope.query_company_month_res_timelist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.datasource = res.body.data;
            $scope.column = [
                {
                    title: '时间',
                    dataIndex: 'dtime',
                    sort: 'true',
                    align: 'center'
                }, {
                    title: '辐射总量' + (res.body.units.rad_chk ? '(' + res.body.units.rad_chk + ')' : ''),
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    sort: 'true',
                    align: 'right',
                }
            ];
        };
        // 公司 月 时间 柱状图
        $scope.query_company_month_res_timeline_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.xdata = res.body.data.dtime;
            $scope.ydatas = [[res.body.data.rad_chk, res.body.data.real_hours]];
        };
        // 公司 月 电站 列表
        $scope.query_company_month_res_stationlist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.datasource = res.body.data;
            $scope.column = [
                {
                    title: '电站名称',
                    dataIndex: 'st_name',
                    sort: 'true',
                    width: '40%',
                    align: 'left'
                }, {
                    title: '装机容量' + (res.body.units.inst_kw ? '(' + res.body.units.inst_kw + ')' : ''),
                    dataIndex: 'inst_kw',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '辐射总量' + (res.body.units.rad_chk ? '(' + res.body.units.rad_chk + ')' : ''),
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '降雨量' + (res.body.units.pcpn ? '(' + res.body.units.pcpn + ')' : ''),
                    dataIndex: 'pcpn',
                    sort: 'true',
                    align: 'right'
                },
            ];
        };
        // 公司 月 电站 柱状图
        $scope.query_company_month_res_stationline_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.xdata = res.body.data.st_name;
            $scope.ydatas = [[res.body.data.rad_chk, res.body.data.real_hours]];
        };
        // 公司 年 时间 列表
        $scope.query_company_year_res_timelist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.datasource = res.body.data;
            $scope.column = [
                {
                    title: '时间',
                    dataIndex: 'dtime',
                    sort: 'true',
                    align: 'center'
                }, {
                    title: '辐射总量' + (res.body.units.rad_chk ? '(' + res.body.units.rad_chk + ')' : ''),
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="环比${item.rad_chk_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                }, {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    sort: 'true',
                    align: 'right',
                    render(text, item, index) {
                        return `
                            <span>${text}</span>
                            <i title="环比${item.real_hours_hb_rate || '-'}%" 
                                class="iconfont icon-zhuzhuangtu" 
                                style="color: #2ac2ad;cursor:pointer;"></i>`
                    }
                }
            ];
        };
        // 公司 年 时间 柱状图
        $scope.query_company_year_res_timeline_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.xdata = res.body.data.dtime;
            $scope.ydatas = [[res.body.data.rad_chk, res.body.data.real_hours]];
        };
        // 公司 年 电站 列表
        $scope.query_company_year_res_stationlist_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.datasource = res.body.data;
            $scope.column = [
                {
                    title: '电站名称',
                    dataIndex: 'st_name',
                    sort: 'true',
                    width: '40%',
                    align: 'left'
                }, {
                    title: '装机容量' + (res.body.units.inst_kw ? '(' + res.body.units.inst_kw + ')' : ''),
                    dataIndex: 'inst_kw',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '辐射总量' + (res.body.units.rad_chk ? '(' + res.body.units.rad_chk + ')' : ''),
                    dataIndex: 'rad_chk',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '发电小时数' + (res.body.units.real_hours ? '(' + res.body.units.real_hours + ')' : ''),
                    dataIndex: 'real_hours',
                    sort: 'true',
                    align: 'right',
                }, {
                    title: '降雨量' + (res.body.units.pcpn ? '(' + res.body.units.pcpn + ')' : ''),
                    dataIndex: 'pcpn',
                    sort: 'true',
                    align: 'right',
                },
            ];
        };
        // 公司 年 电站 柱状图
        $scope.query_company_year_res_stationline_his.getDataCallback = (success, res) => {
            if (!success) return;
            $scope.xdata = res.body.data.st_name;
            $scope.ydatas = [[res.body.data.rad_chk, res.body.data.real_hours]];
        };

        // 计算 图表 显示对应 轴
        function diffChartLineData() {
            if (!$scope.initComplete) return;
            const { currentClass, selectOptions, select1, select2 } = $scope;
            if (currentClass === 'StationMonth') {
                $scope.xdata = $scope.query_station_month_res_timeline_his.res.body.data.dtime
                $scope.ydatas = [[$scope.query_station_month_res_timeline_his.res.body.data[select1]], [$scope.query_station_month_res_timeline_his.res.body.data[select2]]]
            } else if (currentClass === 'StationYear') {
                $scope.xdata = $scope.query_station_year_res_timeline_his.res.body.data.dtime
                $scope.ydatas = [[$scope.query_station_year_res_timeline_his.res.body.data[select1]], [$scope.query_station_year_res_timeline_his.res.body.data[select2]]]
            } else if (currentClass === 'CompanyDayAgo') {
                $scope.ydatas = [[$scope.query_company_day_res_timelist_his.res.body.data.map(v => v[$scope.select1])], [$scope.query_company_day_res_timelist_his.res.body.data.map(v => v.max_sun)]];
            }
        }

        // 轴线切换  可选项  判断
        function diffSelectOptions(units = {}) {
            if ($scope.currentClass === 'StationMonth') {
                $scope.selectOptions = {
                    max_sun: '最大辐射强度' + (units.max_sun ? '(' + units.max_sun + ')' : ''),
                    rad_chk: '辐射总量' + (units.rad_chk ? '(' + units.rad_chk + ')' : ''),
                    sun_hours: '日照小时数' + (units.sun_hours ? '(' + units.sun_hours + ')' : ''),
                    ta_avg: '平均温度' + (units.ta_avg ? '(' + units.ta_avg + ')' : ''),
                    real_hours: '发电小时数' + (units.real_hours ? '(' + units.real_hours + ')' : ''),
                    pcpn: '降雨量' + (units.pcpn ? '(' + units.pcpn + ')' : ''),
                };
            } else if ($scope.currentClass === 'StationYear') {
                $scope.selectOptions = {
                    rad_chk: '辐射总量' + (units.rad_chk ? '(' + units.rad_chk + ')' : ''),
                    sun_hours: '日照小时数' + (units.sun_hours ? '(' + units.sun_hours + ')' : ''),
                    ta_avg: '平均温度' + (units.ta_avg ? '(' + units.ta_avg + ')' : ''),
                    real_hours: '发电小时数' + (units.real_hours ? '(' + units.real_hours + ')' : ''),
                    pcpn: '降雨量' + (units.pcpn ? '(' + units.pcpn + ')' : ''),
                };
            } else if ($scope.currentClass === 'CompanyDayAgo') {
                $scope.selectOptions = {
                    inst_kw: '装机容量' + (units.inst_kw ? '(' + units.inst_kw + ')' : ''),
                    rad_chk: '辐射总量' + (units.rad_chk ? '(' + units.rad_chk + ')' : ''),
                    ta_avg: '平均温度' + (units.ta_avg ? '(' + units.ta_avg + ')' : ''),
                }
            }
        }

        // 定时器  定时刷新接口
        const timer = $interval(() => {
            if ($scope.currentClass === 'StationDayToday' || $scope.currentClass === 'CompanyDayToday') diffGetData();
        },10000);
        $scope.$on('$destroy', () => $interval.cancel(timer));

    });