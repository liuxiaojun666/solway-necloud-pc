        data: {},
        later: true,
        onlyLatest: true
    },
    // 集团年资源详情时间曲线
    query_company_year_res_timeline_his: {
        name: 'GETquery_company_year_res_timeline_his',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 集团年资源详情电站列表
    query_company_year_res_stationlist_his: {
        name: 'GETquery_company_year_res_stationlist_his',
        data: {},
