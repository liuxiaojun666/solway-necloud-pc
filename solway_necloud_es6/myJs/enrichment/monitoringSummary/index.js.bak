ajaxData({}, {})('monitoringSummaryCtrl', ['$scope', 'myAjaxData', 'historicalRecord'], ($scope, myAjaxData, historicalRecord) => {
    // 日期更新后回调   range 时间范围（日月年累计）    date 为日期
    $scope.dateUpdated = (range, date) => historicalRecord.set({ dateType: range, date });

    // 当前页面行为记录初始化回调 获取行为记录
    $scope.hsCallback = () => {
        const hr = historicalRecord.get();
        debugger
    };

    $(".monitoringSummary").on('click', '.common-class', function () {
        let arr = $(".common-class");
        angular.forEach(arr, (v, k) => {
            $(v).removeClass('active')
        });
        $(this).addClass('active').siblings().removeClass('active');

        $scope.currentTheme = $(this).attr('data-theme');//当前所选主题        
    })
});