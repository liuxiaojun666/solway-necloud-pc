ajaxData({
    // info
    analysis_company_dust_accum_info: {
        name: 'GETanalysis_company_dust_accum_info',
        data: {},
        later: true
    },
    // 时间 列表
    analysis_company_dust_accum_timelist: {
        name: 'GETanalysis_company_dust_accum_timelist',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 时间 曲线
    analysis_company_dust_accum_timeline: {
        name: 'GETanalysis_company_dust_accum_timeline',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 电站 列表
    analysis_company_dust_accum_stationlist: {
        name: 'GETanalysis_company_dust_accum_stationlist',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 电站 曲线
    analysis_company_dust_accum_stationline: {
        name: 'GETanalysis_company_dust_accum_stationline',
        data: {},
        later: true,
        onlyLatest: true
    },
    actionCompanyCleanSuggest: {
        name: 'GETactionCompanyCleanSuggest',
        data: {},
        later: true
    },
}, {
    __serviceName__: 'cleanCompanyAccumulativeService',
})('cleanCompanyAccumulativeCtrl', ['$scope', 'cleanCompanyAccumulativeService', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { showShape, tabIndex = 0} = historicalRecord.get().themeCleanCompanyAccumulative || {};
        $scope.changeShowShape(showShape || 'list');
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        await $scope.dateUpdated();
        $scope.changeTab(tabIndex);
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData(true);
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => $scope.diffGetData(true);


    // 点击电站名  跳转到电站页面
    $scope.trClick = (item, index, event) => {
        if (!event.target.className.includes('clickable')) return;
        $scope.reloadParentPage({
            stationData: {
                dataType: 0,
                id: item.stid,
                isGroup: "0",
                stClass: '01'
            }
        });
    };

    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData(true);
    };


    // tab 切换
    $scope.changeTab = tabIndex => {
        $scope.tabIndex = tabIndex;
        historicalRecord.set({
            themeCleanCompanyAccumulative: {
                ...historicalRecord.get().themeCleanCompanyAccumulative || {},
                tabIndex
            }
        });
        $scope.diffGetData();
    };

    // 切换显示形式  图表 or 列表
    $scope.changeShowShape = showShape => {
        $scope.showShape = showShape;
        historicalRecord.set({
            themeCleanCompanyAccumulative: {
                ...historicalRecord.get().themeCleanCompanyAccumulative || {},
                showShape
            }
        });
        $scope.diffGetData();
    };

    // 接口请求
    $scope.diffGetData = async (getInfo) => {
        if (!$scope.initComplete) return;
        await myAjaxData.timeout(0);
        setDataNull(getInfo);
        const { showShape, dateTime = {showDate: null}, tabIndex, dateType, dataType } = $scope;
        if (dateType !== 3) return;
        if (dataType <= 0) return;
        if (getInfo) {
            $scope.analysis_company_dust_accum_info.getData({
                queryCode: 'analysis_company_dust_accum_info',
                queryType: 1,
                date: dateTime.showDate
            });
        }
        if (tabIndex === 0 && showShape === 'list') {
            $scope.analysis_company_dust_accum_timelist.getData({
                queryCode: 'analysis_company_dust_accum_timelist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 0 && showShape === 'chart') {
            $scope.analysis_company_dust_accum_timeline.getData({
                queryCode: 'analysis_company_dust_accum_timeline',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'list') {
            $scope.analysis_company_dust_accum_stationlist.getData({
                queryCode: 'analysis_company_dust_accum_stationlist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'chart') {
            $scope.analysis_company_dust_accum_stationline.getData({
                queryCode: 'analysis_company_dust_accum_stationline',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 2) {
            $scope.actionCompanyCleanSuggest.getData({
                dateType: 3,
                queryType: 1,
                start_date: new Date(Date.now() - 1000 * 24 * 60 * 60 * 30).Format('yyyy-MM-dd'),
                date: new Date(Date.now() - 1000 * 24 * 60 * 60).Format('yyyy-MM-dd')
            });
        }
        $scope.$apply();
    };

    // 时间 列表
    $scope.analysis_company_dust_accum_timelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '日期',
                dataIndex: 'dtime',
                align: 'center',
                sort: true,
            },
            {
                title: '清洁指数' + '(' + res.body.units.clean_mark + ')',
                dataIndex: 'clean_mark',
                align: 'right',
                sort: true,
            },
            {
                title: '灰尘损失电量' + '(' + res.body.units.dust_we + ')',
                dataIndex: 'dust_we',
                align: 'right',
                sort: true,
            },
            {
                title: '灰尘损失比例' + '(' + res.body.units.dust_we_r + ')',
                dataIndex: 'dust_we_r',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 时间 曲线
    $scope.analysis_company_dust_accum_timeline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.dtime;
        $scope.ydatas = [[res.body.data.clean_mark]];
    };

    // 电站 列表
    $scope.analysis_company_dust_accum_stationlist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '电站',
                dataIndex: 'st_name',
                align: 'left',
                width: '25%',
                sort: true,
                render(text) {
                    return `<span class="clickable">${text}</span>`
                }
            },
            {
                title: '清洁指数' + '(' + res.body.units.clean_mark + ')',
                dataIndex: 'clean_mark',
                align: 'right',
                sort: true,
            },
            {
                title: '灰尘损失电量' + '(' + res.body.units.dust_we + ')',
                dataIndex: 'dust_we',
                align: 'right',
                sort: true,
            },
            {
                title: '灰尘损失比例' + '(' + res.body.units.dust_we_r + ')',
                dataIndex: 'dust_we_r',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 电站 曲线
    $scope.analysis_company_dust_accum_stationline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.st_name;
        $scope.ydatas = [[res.body.data.dust_we_r]];
    };


    $scope.actionCompanyCleanSuggest.subscribe(res => {
        $scope.datasource = res.body;
        $scope.column = [
            {
                title: '电站名称',
                dataIndex: 'st_name',
                align: 'left',
                sort: true,
                width: '25%',
                render(text) {
                    return `<span class="clickable">${text}</span>`
                }
            },
            {
                title: '装机容量(kWh)',
                dataIndex: 'inst_kw',
                align: 'right',
                sort: true,
            },
            {
                title: '当前清洁指数(%)',
                dataIndex: 'clean_mark',
                align: 'right',
                sort: true,
            },
            {
                title: '清洁等级',
                dataIndex: 'clean_level',
                align: 'center',
                sort: true,
                render(text) {
                    return `<span style="color: ${[
                        'rgba(80, 182, 118, 1)',
                        'rgba(80, 182, 118, 1)',
                        'rgba(80, 182, 118, 1)',
                        '#FF732E',
                        '#FF732E',
                        '#F75D5D'
                    ][text]}">${['', '优', '良', '轻度积灰', '中度积灰', '严重积灰'][text] || ''}</span>`;
                }
            },
            {
                title: '清洗建议',
                dataIndex: 'pre_clean_status',
                align: 'left',
                width: '30%',
                sort: true,
                render(text, item, index) {
                    if (text === 0) return '<span style="color:rgba(80, 182, 118, 1);">不用清洗</span>'
                    else if (text === 1) return `<span style="color: #F75D5D;">
                            建议清洗，电池板清洗后，按照最近30天的光照条件，预计可提高发电量
                            ${item.dust_we.data.dust_we}${item.dust_we.units.dust_we}
                        </span>`;
                    return '';
                }
            },
            {
                title: '累计清洗次数',
                dataIndex: 'ccn',
                align: 'right',
                sort: true,
            },
        ];

    });


    // 数据置空
    function setDataNull(getInfo) {
        $scope.column = [];
        $scope.xdata = void 0;
        $scope.ydatas = void 0;
        $scope.analysis_company_dust_accum_timelist.res = null;
        $scope.analysis_company_dust_accum_timeline.res = null;
        $scope.analysis_company_dust_accum_stationlist.res = null;
        $scope.analysis_company_dust_accum_stationline.res = null;
        $scope.actionCompanyCleanSuggest.res = null;
        if (getInfo) $scope.analysis_company_dust_accum_info.res = null;
    }

    // 销毁 事件
    $scope.$on('$destroy', () => {
        setDataNull();
    });
});