ajaxData({
    // info
    analysis_wp_station_fail_year_info: {
        name: 'GETanalysis_wp_station_fail_year_info',
        data: {},
        later: true
    },
    // 时间 列表
    analysis_wp_station_fail_year_timelist: {
        name: 'GETanalysis_wp_station_fail_year_timelist',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 时间 曲线
    analysis_wp_station_fail_year_timeline: {
        name: 'GETanalysis_wp_station_fail_year_timeline',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 设备 列表
    analysis_wp_station_fail_month_devicelist: {
        name: 'GETanalysis_wp_station_fail_month_devicelist',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 设备 曲线
    analysis_wp_station_fail_month_deviceline: {
        name: 'GETanalysis_wp_station_fail_month_deviceline',
        data: {},
        later: true,
        onlyLatest: true
    },
}, {
    __serviceName__: 'faultStationYearWpService',
})('faultStationYearWpCtrl', ['$scope', 'faultStationYearWpService', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { showShape, tabIndex = 0 } = historicalRecord.get().themeFaultStationYear || {};
        $scope.changeShowShape(showShape || 'list');
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        await $scope.dateUpdated();
        $scope.changeTab(tabIndex);
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData(true);
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => $scope.diffGetData(true);


    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData(true);
    };


    // tab 切换
    $scope.changeTab = tabIndex => {
        $scope.tabIndex = tabIndex;
        historicalRecord.set({
            themeFaultStationYear: {
                ...historicalRecord.get().themeFaultStationYear || {},
                tabIndex
            }
        });
        $scope.diffGetData();
    };

    // 切换显示形式  图表 or 列表
    $scope.changeShowShape = showShape => {
        $scope.showShape = showShape;
        historicalRecord.set({
            themeFaultStationYear: {
                ...historicalRecord.get().themeFaultStationYear || {},
                showShape
            }
        });
        $scope.diffGetData();
    };

    // 接口请求
    $scope.diffGetData = async (getInfo) => {
        if (!$scope.initComplete) return;
        await myAjaxData.timeout(0);
        setDataNull(getInfo);
        const { showShape, dateTime, tabIndex, dateType, dataType } = $scope;
        if (dateType !== 2) return;
        if (dataType !== 0) return;
        if (getInfo) {
            $scope.analysis_wp_station_fail_year_info.getData({
                queryCode: 'analysis_wp_station_fail_year_info',
                queryType: 3,
                date: dateTime.showDate
            });
        }
        if (tabIndex === 0 && showShape === 'list') {
            $scope.analysis_wp_station_fail_year_timelist.getData({
                queryCode: 'analysis_wp_station_fail_year_timelist',
                queryType: 3,
                date: dateTime.showDate
            });
        } else if (tabIndex === 0 && showShape === 'chart') {
            $scope.analysis_wp_station_fail_year_timeline.getData({
                queryCode: 'analysis_wp_station_fail_year_timeline',
                queryType: 3,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'list') {
            $scope.analysis_wp_station_fail_month_devicelist.getData({
                queryCode: 'analysis_wp_station_fail_month_devicelist',
                queryType: 3,
                date: dateTime.showDate,
                fail_status: 1
            });
        } else if (tabIndex === 1 && showShape === 'chart') {
            $scope.analysis_wp_station_fail_month_deviceline.getData({
                queryCode: 'analysis_wp_station_fail_month_deviceline',
                queryType: 3,
                sort: 'fail_we_r,desc',
                date: dateTime.showDate,
                fail_status: 1
            });
        }
    };

    // 时间 列表
    $scope.analysis_wp_station_fail_year_timelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '日期',
                dataIndex: 'dtime',
                align: 'center',
                sort: true,
            },
            {
                title: '实发电量' + '(' + res.body.units.real_kwh + ')',
                dataIndex: 'real_kwh',
                align: 'right',
                sort: true,
            },
            {
                title: '故障损失电量' + '(' + res.body.units.fail_we + ')',
                dataIndex: 'fail_we',
                align: 'right',
                sort: true,
            },
            {
                title: '故障损失比例' + '(' + res.body.units.fail_we_r + ')',
                dataIndex: 'fail_we_r',
                align: 'right',
                sort: true,
            },
            {
                title: '故障时间' + '(' + res.body.units.fail_hours + ')',
                dataIndex: 'fail_hours',
                align: 'right',
                sort: true,
            },
            {
                title: '故障损失小时数' + '(' + res.body.units.fail_we_hours + ')',
                dataIndex: 'fail_we_hours',
                align: 'right',
                sort: true,
            },
            {
                title: 'mttr' + '(' + res.body.units.mttr + ')',
                dataIndex: 'mttr',
                align: 'right',
                sort: true,
                width: '6%',
            },
            {
                title: 'mtbf' + '(' + res.body.units.mtbf + ')',
                dataIndex: 'mtbf',
                align: 'right',
                sort: true,
                width: '6%',
            },
        ];
    };

    // 时间 曲线
    $scope.analysis_wp_station_fail_year_timeline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.dtime;
        $scope.ydatas = [[res.body.data.real_kwh, res.body.data.fail_we], [res.body.data.fail_we_r]];
        $scope.ydatas2 = [[res.body.data.mttr], [res.body.data.mtbf]];
    };

    // 设备故障， 查看设备详情
    $scope.equipmentDetailruning = (item, index, event) => {
        if (!event.target.className.includes('clickable')) return;
        const { stid, eqid } = item;
        $scope.$broadcast("equipmentPopUpWp", {
            stid,
            eqid
        });
    };

    // 设备 列表
    $scope.analysis_wp_station_fail_month_devicelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '设备',
                dataIndex: 'eq_name',
                align: 'left',
                width: '20%',
                sort: true,
                render(text) {
                    return `<span class="clickable">${text}</span>`
                }
            },
            {
                title: '装机容量' + '(' + res.body.units.inst_kw + ')',
                dataIndex: 'inst_kw',
                align: 'right',
                sort: true,
            },
            {
                title: '实发电量' + '(' + res.body.units.real_kwh + ')',
                dataIndex: 'real_kwh',
                align: 'right',
                sort: true,
            },
            {
                title: '故障损失电量' + '(' + res.body.units.fail_we + ')',
                dataIndex: 'fail_we',
                align: 'right',
                sort: true,
            },
            {
                title: '故障损失比例' + '(' + res.body.units.fail_we_r + ')',
                dataIndex: 'fail_we_r',
                align: 'right',
                sort: true,
            },
            {
                title: '故障时间' + '(' + res.body.units.fail_hours + ')',
                dataIndex: 'fail_hours',
                align: 'right',
                sort: true,
            },
            {
                title: '故障损失小时数' + '(' + res.body.units.fail_we_hours + ')',
                dataIndex: 'fail_we_hours',
                align: 'right',
                sort: true,
            },
            {
                title: 'mttr' + '(' + res.body.units.mttr + ')',
                dataIndex: 'mttr',
                align: 'right',
                sort: true,
                width: '6%',
            },
            {
                title: 'mtbf' + '(' + res.body.units.mtbf + ')',
                dataIndex: 'mtbf',
                align: 'right',
                sort: true,
                width: '6%',
            },
        ];
    };

    // 设备 曲线
    $scope.analysis_wp_station_fail_month_deviceline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.eq_name;
        $scope.ydatas = [[res.body.data.real_kwh, res.body.data.fail_we], [res.body.data.fail_we_r]];
        $scope.ydatas2 = [[res.body.data.mttr], [res.body.data.mtbf]];
    };

    // 数据置空
    function setDataNull(getInfo) {
        $scope.column = [];
        $scope.xdata = void 0;
        $scope.ydatas = void 0;
        $scope.ydatas2 = void 0;
        $scope.analysis_wp_station_fail_year_timelist.res = null;
        $scope.analysis_wp_station_fail_year_timeline.res = null;
        $scope.analysis_wp_station_fail_month_devicelist.res = null;
        $scope.analysis_wp_station_fail_month_deviceline.res = null;
        if (getInfo) $scope.analysis_wp_station_fail_year_info.res = null;
    }

    // 销毁 事件
    $scope.$on('$destroy', () => {
        setDataNull();
    });
});