ajaxData({
    // info
    analysis_wp_company_eval_fra_accum_info: {
        name: 'GETanalysis_wp_company_eval_fra_accum_info',
        data: {},
        later: true
    },
    // 时间 列表
    analysis_wp_company_eval_fra_accum_timelist: {
        name: 'GETanalysis_wp_company_eval_fra_accum_timelist',
        data: {},
        later: true,
        onlyLatest: true
    },
    // 时间 曲线
    analysis_wp_company_eval_fra_accum_timeline: {
        name: 'GETanalysis_wp_company_eval_fra_accum_timeline',
        data: {},
        later: true,
        onlyLatest: true
    },
}, {
    __serviceName__: 'resourcesDeviationCompanyAccumulativeWpService',
})('resourcesDeviationCompanyAccumulativeWpCtrl', ['$scope', 'resourcesDeviationCompanyAccumulativeWpService', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { showShape } = historicalRecord.get().themeResourcesDeviationCompanyAccum || {};
        $scope.changeShowShape(showShape || 'list');
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        await $scope.dateUpdated();
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData(true);
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => $scope.diffGetData(true);


    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData(true);
    };

    // 切换显示形式  图表 or 列表
    $scope.changeShowShape = showShape => {
        $scope.showShape = showShape;
        historicalRecord.set({
            themeResourcesDeviationCompanyAccum: {
                ...historicalRecord.get().themeResourcesDeviationCompanyAccum || {},
                showShape
            }
        });
        $scope.diffGetData();
    };

    // 接口请求
    $scope.diffGetData = async (getInfo) => {
        if (!$scope.initComplete) return;
        await myAjaxData.timeout(0);
        setDataNull(getInfo);
        const { showShape, dateTime = {showDate: null}, tabIndex, dateType, dataType } = $scope;
        if (dateType !== 3) return;
        if (dataType <= 0) return;
        if (getInfo) {
            $scope.analysis_wp_company_eval_fra_accum_info.getData({
                queryCode: 'analysis_wp_company_eval_fra_accum_info',
                queryType: 1,
                date: dateTime.showDate
            });
        }
        if (showShape === 'list') {
            $scope.analysis_wp_company_eval_fra_accum_timelist.getData({
                queryCode: 'analysis_wp_company_eval_fra_accum_timelist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (showShape === 'chart') {
            $scope.analysis_wp_company_eval_fra_accum_timeline.getData({
                queryCode: 'analysis_wp_company_eval_fra_accum_timeline',
                queryType: 1,
                date: dateTime.showDate
            });
        }
    };

    // 时间 列表
    $scope.analysis_wp_company_eval_fra_accum_timelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '日期',
                dataIndex: 'dtime',
                align: 'center',
                sort: true,
            },
            {
                title: '评估电量' + '(' + res.body.units.eval_kwh + ')',
                dataIndex: 'eval_kwh',
                align: 'right',
                sort: true,
            },
            {
                title: '理论电量' + '(' + res.body.units.fra_kwh + ')',
                dataIndex: 'fra_kwh',
                align: 'right',
                sort: true,
            },
            {
                title: '资源偏差' + '(' + res.body.units.res_dev_we_r + ')',
                dataIndex: 'res_dev_we_r',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 时间 曲线
    $scope.analysis_wp_company_eval_fra_accum_timeline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.dtime;
        $scope.ydatas = [[res.body.data.eval_kwh, res.body.data.fra_kwh], [res.body.data.res_dev_we_r]];
        // $scope.ydatas2 = [[res.body.data.eval_rad_d, res.body.data.rad_chk], [res.body.data.res_dev_rad_r]];
    };

    // 数据置空
    function setDataNull(getInfo) {
        $scope.column = [];
        $scope.xdata = void 0;
        $scope.ydatas = void 0;
        $scope.ydatas2 = void 0;
        $scope.analysis_wp_company_eval_fra_accum_timelist.res = null;
        $scope.analysis_wp_company_eval_fra_accum_timeline.res = null;
        if (getInfo) $scope.analysis_wp_company_eval_fra_accum_info.res = null;
    }

    // 销毁 事件
    $scope.$on('$destroy', () => {
        setDataNull();
    });
});