ajaxData(
    {
        // 时间 列表
        analysis_wp_company_power_exp_year_timelist: {
            name: 'GETanalysis_wp_company_power_exp_year_timelist',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 时间 曲线
        analysis_wp_company_power_exp_year_timeline: {
            name: 'GETanalysis_company_pgc_year_timeline',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 电站 列表
        analysis_wp_company_power_exp_year_stationlist: {
            name: 'GETanalysis_wp_company_power_exp_year_stationlist',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 电站 曲线
        analysis_wp_company_power_exp_year_stationline: {
            name: 'GETanalysis_wp_company_power_exp_year_stationline',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 设备 列表
        analysis_wp_company_power_exp_year_devicelist: {
            name: 'GETanalysis_wp_company_power_exp_year_devicelist',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 设备 曲线
        analysis_wp_company_power_exp_year_deviceline: {
            name: 'GETanalysis_wp_company_power_exp_year_deviceline',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 查设备型号
        queryProducts: {
            name: 'GETqueryProducts',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 多个风机--风速-功率-月-每台风机曲线：支持跨电站设备比较
        selectTurbineWSPMonthLine: {
            name: 'GETselectTurbineWSPMonthLine',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 单个风机--列表
        analysis_wp_device_power_exp_year_timelist: {
            name: 'GETanalysis_wp_device_power_exp_year_timelist',
            data: {},
            later: true
        },
        // 单个风机--风速-功率-月-每天曲线
        getTurbineWSPDayLine: {
            name: 'GETgetTurbineWSPDayLine',
            data: {},
            later: true,
            onlyLatest: true
        },
    }, {
        __serviceName__: 'powerCurveCompanyYearService',
    }
)('powerCurveCompanyYearCtrl', ['$scope', 'powerCurveCompanyYearService', 'actionRecord', 'toaster'], ($scope, myAjaxData, historicalRecord, toaster) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { showShape, tabIndex = 0 } = historicalRecord.get().themePowerCurveCompanyYear || {};
        $scope.changeShowShape(showShape || 'list');
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        await $scope.dateUpdated();
        $scope.changeTab(tabIndex);
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData(true);
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => $scope.diffGetData(true);

    // 点击电站名  跳转到电站页面
    $scope.trClick = (item, index, event) => {
        const {tabIndex} = $scope;
        if (tabIndex === 0) return;
        else if (tabIndex === 1) {
            if (!event.target.className.includes('clickable')) return;
            return $scope.reloadParentPage({
                stationData: {
                    dataType: 0,
                    id: item.stid,
                    isGroup: "0",
                    stClass: '02'
                }
            });
        }
        else if (tabIndex === 2) {
            if ($scope.selectTurbineWSPMonthLine.pageShow) return $scope.selectTurbineWSPMonthLine.trClick(item, index, event);
            
            if (event.target.className.includes('eq')) {
                const { stid, eqid } = item;
                return $scope.$broadcast("equipmentPopUpWp", { stid, eqid });
            } else if (event.target.className.includes('station')) return $scope.reloadParentPage({
                stationData: { dataType: 0, id: item.stid, isGroup: "0", stClass: '02' }
            });
        }
        
    };


    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData(true);
    };

    // tab 切换
    $scope.changeTab = tabIndex => {
        $scope.tabIndex = tabIndex;
        historicalRecord.set({
            themePowerCurveCompanyYear: {
                ...historicalRecord.get().themePowerCurveCompanyYear || {},
                tabIndex
            }
        });
        $scope.diffGetData();
    };

    // 切换显示形式  图表 or 列表
    $scope.changeShowShape = showShape => {
        $scope.showShape = showShape;
        historicalRecord.set({
            themePowerCurveCompanyYear: {
                ...historicalRecord.get().themePowerCurveCompanyYear || {},
                showShape
            }
        });
        $scope.diffGetData();
    };

    // 接口请求
    $scope.diffGetData = async () => {
        if (!$scope.initComplete) return;
        await myAjaxData.timeout(0);
        setDataNull();
        const { showShape, dateTime, dateType, dataType, tabIndex } = $scope;
        if (dateType !== 2) return;
        if (dataType <= 0) return;
        if (tabIndex === 0 && showShape === 'list') {
            $scope.analysis_wp_company_power_exp_year_timelist.getData({
                queryCode: 'analysis_wp_company_power_exp_year_timelist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 0 && showShape === 'chart') {
            $scope.analysis_wp_company_power_exp_year_timeline.getData({
                queryCode: 'analysis_wp_company_power_exp_year_timeline',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'list') {
            $scope.analysis_wp_company_power_exp_year_stationlist.getData({
                queryCode: 'analysis_wp_company_power_exp_year_stationlist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'chart') {
            $scope.analysis_wp_company_power_exp_year_stationline.getData({
                queryCode: 'analysis_wp_company_power_exp_year_stationline',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 2 && showShape === 'list') {
            if (!$scope.queryProducts.res) {
                const res = await $scope.queryProducts.getData({
                    deviceType: 18
                });
                $scope.analysis_wp_company_power_exp_year_devicelist.productId = res.body[0].productId;
            }
            $scope.analysis_wp_company_power_exp_year_devicelist.getData({
                queryCode: 'analysis_wp_company_power_exp_year_devicelist',
                queryType: 1,
                date: dateTime.showDate,
                productid: $scope.analysis_wp_company_power_exp_year_devicelist.productId
            });
        } else if (tabIndex === 2 && showShape === 'chart') {
            $scope.analysis_wp_company_power_exp_year_deviceline.getData({
                queryCode: 'analysis_wp_company_power_exp_year_deviceline',
                queryType: 1,
                sort: 'power_exp_avg,desc',
                date: dateTime.showDate,
            });
        }
    };


    // 时间 列表
    $scope.analysis_wp_company_power_exp_year_timelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '日期',
                dataIndex: 'dtime',
                align: 'center',
                sort: true,
            },
            {
                title: '最大指数',
                dataIndex: 'power_exp_max',
                align: 'right',
                sort: true,
            },
            {
                title: '最小指数',
                dataIndex: 'power_exp_min',
                align: 'right',
                sort: true,
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均温度' + '(' + res.body.units.ta_avg + ')',
                dataIndex: 'ta_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '指数离散率' + '(' + res.body.units.power_exp_cdr + ')',
                dataIndex: 'power_exp_cdr',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 时间 曲线
    $scope.analysis_wp_company_power_exp_year_timeline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.dtime;
        $scope.ydatas = [[res.body.data.power_exp_avg], [res.body.data.ta_avg]];
    };

    // 电站 列表
    $scope.analysis_wp_company_power_exp_year_stationlist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '电站',
                dataIndex: 'st_name',
                align: 'left',
                width: '25%',
                sort: true,
                render(text) {
                    return `<span class="clickable">${text}</span>`
                }
            },
            {
                title: '最大指数',
                dataIndex: 'power_exp_max',
                align: 'right',
                sort: true,
            },
            {
                title: '最小指数',
                dataIndex: 'power_exp_min',
                align: 'right',
                sort: true,
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均温度' + '(' + res.body.units.ta_avg + ')',
                dataIndex: 'ta_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '指数离散率' + '(' + res.body.units.power_exp_cdr + ')',
                dataIndex: 'power_exp_cdr',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 电站 曲线
    $scope.analysis_wp_company_power_exp_year_stationline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.st_name;
        $scope.ydatas = [[res.body.data.power_exp_avg], [res.body.data.ta_avg]];
    };

    // 设备 列表
    $scope.analysis_wp_company_power_exp_year_devicelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '<label class="i-checks m-b-none"><input type="checkbox"><i style="background:transparent;margin-right:0;"></i></label>',
                dataIndex: 'eqid',
                align: 'center',
                width: '50px',
                checkboxName: 'batchprocessing',
                render(text, item) {
                    return `<label class="i-checks m-b-none">
                                <input value="${item.stid + '-' + text}" data-identifies="${item.stid + '-' + text}" name="batchprocessing" type="checkbox" />
                                <i style="background:transparent;"></i>
                            </label>`;
                }
            },
            // {
            //     title: '电站',
            //     dataIndex: 'st_name',
            //     align: 'left',
            //     width: '20%',
            //     sort: true,
            //     render(text) {
            //         return `<span class="clickable station">${text}</span>`
            //     }
            // },
            {
                title: '设备',
                dataIndex: 'eq_name',
                align: 'left',
                width: '20%',
                // sort: true,
                render(text, item) {
                    return `<span class="clickable station">${item.st_name}</span>` 
                            +
                            '-'
                            +
                            `<span class="clickable eq">${text}</span>`
                }
            },
            {
                title: '最大指数',
                dataIndex: 'power_exp_max',
                align: 'right',
                sort: true,
            },
            {
                title: '最小指数',
                dataIndex: 'power_exp_min',
                align: 'right',
                sort: true,
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均温度' + '(' + res.body.units.ta_avg + ')',
                dataIndex: 'ta_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '指数离散率' + '(' + res.body.units.power_exp_cdr + ')',
                dataIndex: 'power_exp_cdr',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 设备 曲线
    $scope.analysis_wp_company_power_exp_year_deviceline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.st_name.map((v, i) => v + '\n' + res.body.data.eq_name[i]);
        $scope.ydatas = [[res.body.data.power_exp_avg], [res.body.data.ta_avg]];
    };

    // 不同设备对比
    $scope.analysis_wp_company_power_exp_year_devicelist.comparison = () => {
        const ids = document.getElementsByName('batchprocessing'):: [].filter(v => v.checked).map(v => v.value);
        if (ids.length === 0) return toaster.pop('info', '', '请勾选要对比的数据。');
        $scope.selectTurbineWSPMonthLine.pageShow = true;
        $scope.selectTurbineWSPMonthLine.getData({
            st_eqids: ids.join(';'),
            queryType: 1,
            date: $scope.dateTime.showDate,
            dateType: 1
        });
    };

    // 不同设备对比图
    $scope.selectTurbineWSPMonthLine.subscribe(res => {
        const keys = Object.keys(res.body.names);
        const names = Object.values(res.body.names);
        const idIndex = names.findIndex(v => v === $scope.selectTurbineWSPMonthLine.activeEqName);
        $scope.selectTurbineWSPMonthLine.chart = {
            types: [new Array(keys.length).fill('line')],
            xdata: res.body.x,
            linenames: [names],
            ydatas: [keys.map(v => res.body[v].slice(0, res.body.x.length))],
            z: [keys.map((v, i) => i === idIndex ? 3 : 2)],
            colors: [keys.map((v, i) => i === idIndex ? '#FF6060' : '#fff')]
        };
    });

    // 不同设备对比图 点击行
    $scope.selectTurbineWSPMonthLine.trClick = (item, index, event) => {
        const ids = Object.keys($scope.selectTurbineWSPMonthLine.res.body.names);
        const idIndex = ids.findIndex(v => v === (item.stid + '-' + item.eqid));
        if (idIndex < 0) return;
        event && $(event.target).parents('tr').css('background', '#1b4980').siblings().css('background', '');
        $scope.selectTurbineWSPMonthLine.chart = {
            ...$scope.selectTurbineWSPMonthLine.chart,
            z: [ids.map((v, i) => i === idIndex ? 3 : 2)],
            colors: [ids.map((v, i) => i === idIndex ? '#FF6060' : '#fff')],
        };
        $scope.selectTurbineWSPMonthLine.activeEqName = item.st_name + '-' + item.eq_name;
    };

    // 不同设备对比图 tooltip
    $scope.selectTurbineWSPMonthLine.tooltipFormatter = (params) => {
        const { activeEqName } = $scope.selectTurbineWSPMonthLine;
        return '风速：' + params[0].name + 'm/s<br/>' + params.sort((a, b) => b.value - a.value).map(v => `<span style="color:${v.seriesName === activeEqName ? '#FF6060' : '#fff'};">` + v.seriesName + '-功率(kW):    ' + v.value + '</span>' + '<br/>').join('');
    };

    // 同一设备 对比
    $scope.analysis_wp_company_power_exp_year_devicelist.trDblclick = async (item, index, event) => {
        if ($scope.tabIndex !== 2) return;
        if ($scope.selectTurbineWSPMonthLine.pageShow) return;
        $scope.getTurbineWSPDayLine.pageShow = true;
        $scope.selectTurbineWSPMonthLine.eq_name = item.st_name + '-' + item.eq_name;
        $scope.getTurbineWSPDayLine.chart = null;
        $scope.analysis_wp_device_power_exp_year_timelist.datasource = null;
        await myAjaxData.timeout(200);
        $scope.$apply();
        const {stid, eqid} = item;
        await $scope.getTurbineWSPDayLine.getData({
            stid,
            eqid,
            queryType: 3,
            date: $scope.dateTime.showDate,
            dateType: 1
        });
        $scope.analysis_wp_device_power_exp_year_timelist.getData({
            queryCode: 'analysis_wp_device_power_exp_year_timelist',
            queryType: 3,
            date: $scope.dateTime.showDate,
            stid,
            eqid
        });
    };

    // 同一设备对比 列表
    $scope.analysis_wp_device_power_exp_year_timelist.subscribe(res => {
        $scope.analysis_wp_device_power_exp_year_timelist.datasource = res.body.data;
        $scope.analysis_wp_device_power_exp_year_timelist.column = [
            {
                title: '<label class="i-checks m-b-none"><input checked type="checkbox"><i style="background:transparent;margin-right:0;"></i></label>',
                dataIndex: 'eqid',
                align: 'center',
                width: '50px',
                checkboxName: 'batchprocessing2',
                render(text, item) {
                    return `<label class="i-checks m-b-none">
                                <input checked value="${item.dtime}" data-identifies="${item.dtime}" name="batchprocessing2" type="checkbox" />
                                <i style="background:transparent;"></i>
                            </label>`;
                }
            },
            {
                title: '日期',
                dataIndex: 'dtime',
                align: 'center',
                sort: true
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true
            }
        ];
    });

    // 同一设备对比 曲线
    $scope.analysis_wp_device_power_exp_year_timelist.subscribe(res => {
        const { body } = $scope.getTurbineWSPDayLine.res;
        $scope.getTurbineWSPDayLine.activeEqName = '';
        $scope.getTurbineWSPDayLine.chart = {
            types: [res.body.data.map(v => 'line')],
            xdata: body.x,
            linenames: [res.body.data.map(v => v.dtime)],
            ydatas: [res.body.data.map(v => body[v.dtime])],
            colors: [res.body.data.map(v => '#fff')],
            z: [res.body.data.map(v => 2)],
        };
    });

    // 同一设备对比 CheckBox
    $scope.analysis_wp_device_power_exp_year_timelist.checkboxOnchange = (_all, elements) => {
        const dtimes = Array.from(elements).filter(v => v.checked).map(v => v.value);
        const { body } = $scope.getTurbineWSPDayLine.res;
        const idIndex = dtimes.findIndex(v => v === $scope.getTurbineWSPDayLine.activeEqName);
        $scope.getTurbineWSPDayLine.chart = {
            types: [dtimes.map(v => 'line')],
            xdata: body.x,
            linenames: [dtimes],
            ydatas: [dtimes.map(v => body[v])],
            colors: [dtimes.map((v, i) => idIndex === i ? '#FF6060' : '#fff')],
            z: [dtimes.map((v, i) => idIndex === i ? 3 : 1)],
        };
        $scope.$apply();
    };

    // 同一设备对比 列表点击行
    $scope.analysis_wp_device_power_exp_year_timelist.trClick = (item, index, event) => {
        event && $(event.target).parents('tr').css('background', 'rgba(22,46,71,0.3)').siblings().css('background', '');
        const ids = document.getElementsByName('batchprocessing2'):: [].filter(v => v.checked).map(v => v.value);
        const idIndex = ids.findIndex(v => v === item.dtime);
        $scope.getTurbineWSPDayLine.chart = {
            ...$scope.getTurbineWSPDayLine.chart,
            z: [ids.map((v, i) => i === idIndex ? 3 : 2)],
            colors: [ids.map((v, i) => i === idIndex ? '#FF6060' : '#fff')],
        };
        $scope.getTurbineWSPDayLine.activeEqName = item.dtime;
    };

    // 同一设备对比 tooltip
    $scope.getTurbineWSPDayLine.tooltipFormatter = (params) => {
        const { activeEqName } = $scope.getTurbineWSPDayLine;
        return '风速：' + params[0].name + 'm/s<br/>' + params.sort((a, b) => b.value - a.value).map(v => `<span style="color:${v.seriesName === activeEqName ? '#FF6060' : '#fff'};">` + v.seriesName + '-功率(kW):    ' + v.value + '</span>' + '<br/>').join('');
    };

    // 数据置空
    function setDataNull() {
        $scope.column = [];
        $scope.xdata = void 0;
        $scope.ydatas = void 0;
        $scope.analysis_wp_company_power_exp_year_timelist.res = null;
        $scope.analysis_wp_company_power_exp_year_timeline.res = null;
        $scope.analysis_wp_company_power_exp_year_stationlist.res = null;
        $scope.analysis_wp_company_power_exp_year_stationline.res = null;
        $scope.analysis_wp_company_power_exp_year_devicelist.res = null;
        $scope.analysis_wp_company_power_exp_year_deviceline.res = null;
        $scope.selectTurbineWSPMonthLine.pageShow = false;
    }

    // 销毁 事件
    $scope.$on('$destroy', () => {
        setDataNull();
    });
});