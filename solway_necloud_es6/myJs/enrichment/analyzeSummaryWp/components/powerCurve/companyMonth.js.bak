ajaxData(
    {
        // 时间 列表
        analysis_wp_company_power_exp_month_timelist: {
            name: 'GETanalysis_wp_company_power_exp_month_timelist',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 时间 曲线
        analysis_wp_company_power_exp_month_timeline: {
            name: 'GETanalysis_company_pgc_month_timeline',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 电站 列表
        analysis_wp_company_power_exp_month_stationlist: {
            name: 'GETanalysis_wp_company_power_exp_month_stationlist',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 电站 曲线
        analysis_wp_company_power_exp_month_stationline: {
            name: 'GETanalysis_wp_company_power_exp_month_stationline',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 设备 列表
        analysis_wp_company_power_exp_month_devicelist: {
            name: 'GETanalysis_wp_company_power_exp_month_devicelist',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 设备 曲线
        analysis_wp_company_power_exp_month_deviceline: {
            name: 'GETanalysis_wp_company_power_exp_month_deviceline',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 查设备型号
        queryProducts: {
            name: 'GETqueryProducts',
            data: {},
            later: true,
            onlyLatest: true
        },
        // 多个风机--风速-功率-月-每台风机曲线：支持跨电站设备比较
        selectTurbineWSPMonthLine: {
            name: 'GETselectTurbineWSPMonthLine',
            data: {},
            later: true
        }
    }, {
        __serviceName__: 'powerCurveCompanyMonthService',
    }
)('powerCurveCompanyMonthCtrl', ['$scope', 'powerCurveCompanyMonthService', 'actionRecord', 'toaster'], ($scope, myAjaxData, historicalRecord, toaster) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        let { showShape, tabIndex = 0 } = historicalRecord.get().themePowerCurveCompanyMonth || {};
        $scope.changeShowShape(showShape || 'list');
        const { dateType, dateTime } = historicalRecord.get();
        $scope.dateType = dateType;
        $scope.dateTime = dateTime;
        await $scope.dateUpdated();
        $scope.changeTab(tabIndex);
        $('.ng-clock').removeClass('ng-clock');
        $scope.initComplete = true;
        await myAjaxData.timeout(0);
        await $scope.diffGetData(true);
        $scope.$apply();
    };


    // 切换电站 回调
    $scope.switchPowerCallback = () => $scope.diffGetData(true);

    // 点击电站名  跳转到电站页面
    $scope.trClick = (item, index, event) => {
        const {tabIndex} = $scope;
        if (tabIndex === 0) return;
        else if (tabIndex === 1) {
            if (!event.target.className.includes('clickable')) return;
            return $scope.reloadParentPage({
                stationData: {
                    dataType: 0,
                    id: item.stid,
                    isGroup: "0",
                    stClass: '02'
                }
            });
        }
        else if (tabIndex === 2) {
            if ($scope.selectTurbineWSPMonthLine.pageShow) return event && $(event.target).parents('tr').css('background', '#1b4980').siblings().css('background', '');
            if (event.target.className.includes('eq')) {
                const { stid, eqid } = item;
                return $scope.$broadcast("equipmentPopUpWp", { stid, eqid });
            } else if (event.target.className.includes('station')) return $scope.reloadParentPage({
                stationData: { dataType: 0, id: item.stid, isGroup: "0", stClass: '02' }
            });
        }
        
    };


    // 日期改变
    $scope.dateUpdated = async () => {
        await myAjaxData.timeout(0);
        $scope.diffGetData(true);
    };

    // tab 切换
    $scope.changeTab = tabIndex => {
        $scope.tabIndex = tabIndex;
        historicalRecord.set({
            themePowerCurveCompanyMonth: {
                ...historicalRecord.get().themePowerCurveCompanyMonth || {},
                tabIndex
            }
        });
        $scope.diffGetData();
    };

    // 切换显示形式  图表 or 列表
    $scope.changeShowShape = showShape => {
        $scope.showShape = showShape;
        historicalRecord.set({
            themePowerCurveCompanyMonth: {
                ...historicalRecord.get().themePowerCurveCompanyMonth || {},
                showShape
            }
        });
        $scope.diffGetData();
    };

    // 接口请求
    $scope.diffGetData = async () => {
        if (!$scope.initComplete) return;
        await myAjaxData.timeout(0);
        setDataNull();
        const { showShape, dateTime, dateType, dataType, tabIndex } = $scope;
        if (dateType !== 1) return;
        if (dataType <= 0) return;
        if (tabIndex === 0 && showShape === 'list') {
            $scope.analysis_wp_company_power_exp_month_timelist.getData({
                queryCode: 'analysis_wp_company_power_exp_month_timelist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 0 && showShape === 'chart') {
            $scope.analysis_wp_company_power_exp_month_timeline.getData({
                queryCode: 'analysis_wp_company_power_exp_month_timeline',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'list') {
            $scope.analysis_wp_company_power_exp_month_stationlist.getData({
                queryCode: 'analysis_wp_company_power_exp_month_stationlist',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 1 && showShape === 'chart') {
            $scope.analysis_wp_company_power_exp_month_stationline.getData({
                queryCode: 'analysis_wp_company_power_exp_month_stationline',
                queryType: 1,
                date: dateTime.showDate
            });
        } else if (tabIndex === 2 && showShape === 'list') {
            if (!$scope.queryProducts.res) {
                const res = await $scope.queryProducts.getData({
                    deviceType: 18
                });
                $scope.analysis_wp_company_power_exp_month_devicelist.productId = res.body[0].productId;
            }
            $scope.analysis_wp_company_power_exp_month_devicelist.getData({
                queryCode: 'analysis_wp_company_power_exp_month_devicelist',
                queryType: 1,
                date: dateTime.showDate,
                productid: $scope.analysis_wp_company_power_exp_month_devicelist.productId
            });
        } else if (tabIndex === 2 && showShape === 'chart') {
            $scope.analysis_wp_company_power_exp_month_deviceline.getData({
                queryCode: 'analysis_wp_company_power_exp_month_deviceline',
                queryType: 1,
                date: dateTime.showDate
            });
        }
    };


    // 时间 列表
    $scope.analysis_wp_company_power_exp_month_timelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '日期',
                dataIndex: 'dtime',
                align: 'center',
                sort: true,
            },
            {
                title: '最大指数',
                dataIndex: 'power_exp_max',
                align: 'right',
                sort: true,
            },
            {
                title: '最小指数',
                dataIndex: 'power_exp_min',
                align: 'right',
                sort: true,
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均温度' + '(' + res.body.units.ta_avg + ')',
                dataIndex: 'ta_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均离散率' + '(' + res.body.units.power_exp_cdr + ')',
                dataIndex: 'power_exp_cdr',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 时间 曲线
    $scope.analysis_wp_company_power_exp_month_timeline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.dtime;
        $scope.ydatas = [[res.body.data.power_exp_avg], [res.body.data.ta_avg]];
    };

    // 电站 列表
    $scope.analysis_wp_company_power_exp_month_stationlist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '电站',
                dataIndex: 'st_name',
                align: 'left',
                width: '25%',
                sort: true,
                render(text) {
                    return `<span class="clickable">${text}</span>`
                }
            },
            {
                title: '最大指数',
                dataIndex: 'power_exp_max',
                align: 'right',
                sort: true,
            },
            {
                title: '最小指数',
                dataIndex: 'power_exp_min',
                align: 'right',
                sort: true,
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均温度' + '(' + res.body.units.ta_avg + ')',
                dataIndex: 'ta_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均离散率' + '(' + res.body.units.power_exp_cdr + ')',
                dataIndex: 'power_exp_cdr',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 电站 曲线
    $scope.analysis_wp_company_power_exp_month_stationline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.st_name;
        $scope.ydatas = [[res.body.data.power_exp_avg], [res.body.data.ta_avg]];
    };

    // 设备 列表
    $scope.analysis_wp_company_power_exp_month_devicelist.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.datasource = res.body.data;
        $scope.column = [
            {
                title: '<label class="i-checks m-b-none"><input type="checkbox"><i style="background:transparent;margin-right:0;"></i></label>',
                dataIndex: 'eqid',
                align: 'center',
                width: '50px',
                checkboxName: 'batchprocessing',
                render(text, item) {
                    return `<label class="i-checks m-b-none">
                                <input value="${item.stid + '-' + text}" data-identifies="${item.stid + '-' + text}" name="batchprocessing" type="checkbox" />
                                <i style="background:transparent;"></i>
                            </label>`;
                }
            },
            // {
            //     title: '电站',
            //     dataIndex: 'st_name',
            //     align: 'left',
            //     width: '20%',
            //     sort: true,
            //     render(text) {
            //         return `<span class="clickable station">${text}</span>`
            //     }
            // },
            {
                title: '设备',
                dataIndex: 'eq_name',
                align: 'left',
                width: '20%',
                // sort: true,
                render(text, item) {
                    return `<span class="clickable station">${item.st_name}</span>` 
                            +
                            '-'
                            +
                            `<span class="clickable eq">${text}</span>`
                }
            },
            {
                title: '最大指数',
                dataIndex: 'power_exp_max',
                align: 'right',
                sort: true,
            },
            {
                title: '最小指数',
                dataIndex: 'power_exp_min',
                align: 'right',
                sort: true,
            },
            {
                title: '平均指数',
                dataIndex: 'power_exp_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均温度' + '(' + res.body.units.ta_avg + ')',
                dataIndex: 'ta_avg',
                align: 'right',
                sort: true,
            },
            {
                title: '平均离散率' + '(' + res.body.units.power_exp_cdr + ')',
                dataIndex: 'power_exp_cdr',
                align: 'right',
                sort: true,
            },
        ];
    };

    // 设备 曲线
    $scope.analysis_wp_company_power_exp_month_deviceline.getDataCallback = (success, res) => {
        if (!success) return;
        $scope.xdata = res.body.data.st_name.map((v, i) => v + '\n' + res.body.data.eq_name[i]);
        $scope.ydatas = [[res.body.data.power_exp_avg], [res.body.data.ta_avg]];
    };

    // 对比
    $scope.analysis_wp_company_power_exp_month_devicelist.comparison = () => {

        const ids = document.getElementsByName('batchprocessing'):: [].filter(v => v.checked).map(v => v.value);
        
        if (ids.length === 0) return toaster.pop('info', '', '请勾选要对比的数据。');

        $scope.selectTurbineWSPMonthLine.pageShow = true;

        $scope.selectTurbineWSPMonthLine.getData({
            st_eqids: ids.join(';'),
            queryType: 1,
            date: $scope.dateTime.showDate,
            dateType: 2
        });
    };

    $scope.selectTurbineWSPMonthLine.subscribe(res => {
    });

    // 数据置空
    function setDataNull() {
        $scope.column = [];
        $scope.xdata = void 0;
        $scope.ydatas = void 0;
        $scope.analysis_wp_company_power_exp_month_timelist.res = null;
        $scope.analysis_wp_company_power_exp_month_timeline.res = null;
        $scope.analysis_wp_company_power_exp_month_stationlist.res = null;
        $scope.analysis_wp_company_power_exp_month_stationline.res = null;
        $scope.analysis_wp_company_power_exp_month_devicelist.res = null;
        $scope.analysis_wp_company_power_exp_month_deviceline.res = null;
    }

    // 销毁 事件
    $scope.$on('$destroy', () => {
        setDataNull();
    });
});