ajaxData(
    {

    },
    {
        __serviceName__: 'actionPlanSummaryCompanyEquipmentMedicalService',
    }
)('actionPlanSummaryCompanyEquipmentMedicalCtrl', ['$scope', 'actionPlanSummaryCompanyEquipmentMedicalService', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        $('.ng-clock').removeClass('ng-clock');
    };

    // 切换电站 回调
    $scope.switchPowerCallback = () => {
        if ($scope.dataType === 0) return;
        $scope.diffGetData();
    };

    $scope.column = [
        {
            title: '电站名称',
            dataIndex: '',
            align: 'left',
            width: '30%',
            render(text) {
                return '蒙牛马鞍山'
            }
        },
        {
            title: '待处理故障设备',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '2'
            }
        },
        {
            title: '阴影异常设备',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '3'
            }
        },
        {
            title: '组串离散率异常',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '4'
            }
        },
        {
            title: '衰减异常台数',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '5'
            }
        },
    ];
    
});