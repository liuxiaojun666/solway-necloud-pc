ajaxData(
    {

    },
    {
        __serviceName__: 'actionPlanSummaryCompanyShipmentInspectionManagementService',
    }
)('actionPlanSummaryCompanyShipmentInspectionManagementCtrl', ['$scope', 'actionPlanSummaryCompanyShipmentInspectionManagementService', 'actionRecord'], ($scope, myAjaxData, historicalRecord) => {
    /// 初始化 scope
    $scope.initChildScope($scope, myAjaxData);

    // 主页面初始化完成 回调
    $scope.mainPageInitComplete = async () => {
        $('.ng-clock').removeClass('ng-clock');
    };

    // 切换电站 回调
    $scope.switchPowerCallback = () => {
        if ($scope.dataType === 0) return;
        $scope.diffGetData();
    };
    
    $scope.column = [
        {
            title: '电站名称',
            dataIndex: '',
            align: 'left',
            width: '30%',
            render(text) {
                return '蒙牛马鞍山'
            }
        },
        {
            title: '检修人员数量',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '10'
            }
        },
        {
            title: '两票合格率',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '10'
            }
        },
        {
            title: '检修任务',
            dataIndex: '',
            align: 'right',
            render(text) {
                return '20'
            }
        },
    ];

});